     name : SurListeEffacée
     procedure_id : 1552293935045216738
     type_code : 14
     code : |1-
      // Résumé : Efface et supprime la liste indiquée
      // Syntaxe :
      //SurListeEffacée (<nIDListe> est entier sur 8 octets)
      //
      // Paramètres :
      //	nIDListe (entier sur 8 octets) :ID de la liste
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SurListeEffacée(local nIDListe est un entier sur 8)
      
      	nLigne	est un entier
      		
      	// On recherche dans les autres listes
      	nLigne = ZoneRépétéeCherche(ZR_LISTES_TRI.ATT_ID, nIDListe)
      	SI nLigne > 0 ALORS ZoneRépétéeSupprime(ZR_LISTES_TRI, nLigne)		
      	
      	// On recherche dans les autres listes
      	nLigne = ZoneRépétéeCherche(ZR_LISTES.ATT_ID, nIDListe)
      	SI nLigne > 0 ALORS ZoneRépétéeSupprime(ZR_LISTES, nLigne)		
      	
      	si ZR_LISTES..Occurrence = 0 ALORS
      		
      		// Aide
      		LIB_INFO_LISTE_VIDE..Visible = vrai
      		
      		<COMPILE SI MODE_IOS>
      			IMG_FLECHE_AIDE_IOS..Visible = vrai
      			IMG_FLECHE_AIDE_ANDROID..Visible = Faux
      		<sinon>
      			IMG_FLECHE_AIDE_ANDROID..Visible = vrai
      			IMG_FLECHE_AIDE_IOS..Visible = Faux		
      		<FIN>		
      	FIN
     type : 458752
