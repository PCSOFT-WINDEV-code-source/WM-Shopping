  internal_properties : CAAAAAgAAADU20iqQbWTBV+icvyLacmsiHxj9m6Y3O7I2EaMRVoJQ00WRwXKPPt/PcFWhYBRy+Y45AjjYFL1cS3UnpP2Zbmp4OQPfVz16wHt0Xk5wm2lqo43DyIcD5L33J2V
 code_elements :
  internal_properties : CAAAAAgAAAArFMLlpoDCWvPbdj1zCMWdYTPUfKXXzMuNGUhLxa8O1HpA9hFNXIPh0Z7mWsY9tGZqjvtgqRXWJlDHL6Q0DURY3cj18rA41RaxnYQ79wbiRihrrlEviZXnA3r6vgzKTpHnHWYR
  type_code : 1
  p_codes :
   -
     code : |1-
      PROCEDURE MaFenêtre(local IDListeDéfaut est un entier sur 8 = 0)
      
      SélectionAbandon est un booléen
      
      // Observateurs
      AjouteObservateur(ListeManager.SurListesActualisées, mafenetre..nom, SurListesActualisées)
      AjouteObservateur(ListeManager.SurListeEffacée, MaFenêtre..Nom, SurListeEffacée)
      AjouteObservateur(ListeManager.SurDemandeOuverture, MaFenêtre..Nom, SurDemandeOuverture)
      AjouteObservateur(ListeManager.SurDemandeInfoID, MaFenêtre..Nom, SurDemandeInfoID)
      
      // Aide
      IMG_FLECHE_AIDE_ANDROID..visible = faux
      IMG_FLECHE_AIDE_IOS..visible = faux
      LIB_INFO_LISTE_VIDE..visible = faux
      OPT_CREER_LISTE..Visible = EnModeiOS() 
      
      // Modifie l'image de l'action-bar
      ACTB_MAIN..Image = IMG_MENU
      
      // Titre stylisé
      mafenetre..titre = APPTitre()
   -
     code : |1-
      
      // Plan initial
      MaFenêtre..Plan = 1
      
      <compile si TypeConfiguration = Android>
      	
      	// Ligne de commande
      	si IDListeDéfaut > 0 ALORS
      		
      		// Ouverture de la liste
      		ListeManager.ListeOuvre(IDListeDéfaut, vrai)
      		
      	sinon
      		ListeManager.DéclencheListesActualisées()
      		
      		// Au cas où
      		AidePremièreListe()	
      	FIN
      <sinon>
      	ListeManager.DéclencheListesActualisées()
      	
      	// Au cas où
      	AidePremièreListe()	
      <fin>
     type : 34
   -
     code : |1-
      
      // Fermeture d'une liste
      si MaFenêtreFille..Nom = "FEN_LISTE" ALORS
      	
      	// Met à jour la liste des listes
      	MaFenêtre..Plan = 1
      	ListeManager.DéclencheListesActualisées(faux, faux)
      	
      	// Si il n'y avait qu'une liste, aide à la modification
      	AidePremièreListe()	
      FIN
      
      // Création d'une nouvelle liste
      si MaFenêtreFille..Nom = "FEN_LISTE_CREE" ALORS
      	
      	// On a bien créé une nouvelle liste
      	si MaFenêtreFille..ValeurRenvoyée = vrai ALORS
      		
      		// Met à jour la liste des listes
      		MaFenêtre..Plan = 1
      		ListeManager.DéclencheListesActualisées()			
      						
      		// Une seule liste
      		SI ZR_LISTES..Occurrence = 1 ALORS
      			
      			// Ouverture de la liste
      			ListeManager.ListeOuvre(ZR_LISTES.ATT_ID[1])			
      		sinon
      						
      			// Aide à la modification
      			AidePremièreListe()
      		FIN		
      	FIN	
      FIN
      
      // Création d'une nouvelle liste
      si MaFenêtreFille..Nom = "FEN_LISTE_RENOMME" ALORS
      	
      	// On a bien créé une nouvelle liste
      	si MaFenêtreFille..ValeurRenvoyée = vrai ALORS
      		
      		// Met à jour la liste des listes
      		ListeManager.DéclencheListesActualisées()				
      	FIN	
      FIN
     type : 177
