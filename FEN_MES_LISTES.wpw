     name : SurListesActualisées
     internal_properties : CAAAAAgAAAAutNpBlrS0yaOgLUAq2pWWby2+DBBk/XLU6V0AgXHDHwx1obiaW1pmWVYOaDeYFrTK5kj2Z3OE5x4fPoyerh1cDZnPTlnLVZrFLe3lv5oxegfo18FhY5RURvuR0FcydA+8irI+B9Sk4LUpp1NbvvcNN46jArwd+KzKQoHe9EmNc+RGA8pDnamKnRCjNll85ir7IdOUW/rq5TFh96wmdiJbrPR9t6gE+agKk9jpsOBEXzcrMgvFEiQED5flkdSj2Q==
     procedure_id : 1551867101049888809
     type_code : 14
     code : |1-
      // Résumé : Met à jour l'affichage des listes
      // Syntaxe :
      //SurListesActualisées ( [<void>])
      //
      // Paramètres :
      //	void (valeur par défaut=0) :Vrai si le plan actuel doit rester identique
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SurListesActualisées(void <utile> = null)
      
      	sInfoComptage est une chaine
      	nNbListes	  est un entier	
      	stListe 	  est une stliste
      	
      	// Annule le balayage en cours
      	ZoneRépétéeAnnuleBalayage(ZR_LISTES)
      	
      	// Vide
      	ZoneRépétéeSupprimeTout(ZR_LISTES)
      	
      	// Le tri n'est disponible que sur Android
      	<COMPILE SI TypeConfiguration = Android>
      	ZoneRépétéeSupprimeTout(ZR_LISTES_TRI)
      	<fin>
      	
      	OPT_TERMINER..Visible = (MaFenêtre..Plan = 2)
      		
      	// Option visible en IOS/Android
      	OPT_CREER_LISTE..Visible = (MaFenêtre..Plan = 2 ? Faux SINON EnModeiOS())
      	BTN_AJOUTER_LISTE..Visible = EnModeAndroid() 
      		
      	// Modifie l'image de l'action-bar
      	si mafenetre..plan = 1 ALORS
      		si ACTB_MAIN..Image <> IMG_MENU ALORS
      			ACTB_MAIN..Image = IMG_MENU			
      		FIN
      	sinon
      		si ACTB_MAIN..Image <> IMG_RETOUR ALORS
      			ACTB_MAIN..Image = img_retour			
      		FIN
      	FIN
      	
      	pour tout stListe de MesListes
      	
      		nNbListes ++
      					
      		// Infos
      		si stListe.NbProduits > 0 ALORS
      			sInfoComptage = chaineconstruit(<§Rff0000af112468c2¤fr-FR¤%1 produit%2 à acheter§>, stListe.NbProduits, (stListe.NbProduits > 1 ? "s" sinon ""))
      		
      		sinon
      			sInfoComptage = <§Rff0001af112668c2¤fr-FR¤Aucun produit dans la liste§>
      		FIN
      								
      		// Le tri n'est disponible que sur Android								
      		<compile si TypeConfiguration = Android>
      		ZoneRépétéeAjouteLigne(ZR_LISTES_TRI, 	stListe.Nom, stListe.IDListe)
      		<fin>
      		
      		ZoneRépétéeAjouteLigne(ZR_LISTES, 		(EnModeSimulateur _ou_ stListe.IDFournisseur = 0 ? DATA_IMAGE_SUPPLIER + [fsep] sinon "") + stListe.Image, ...
      												stListe.Nom, sInfoComptage, stListe.IDListe)
      	FIN
      	
      	// Aide
      	bAideVisible est un booléen = (nNbListes <= 0)
      	LIB_INFO_LISTE_VIDE..Visible = bAideVisible
      	
      	<compile si MODE_IOS>
      		IMG_FLECHE_AIDE_IOS..Visible = bAideVisible
      		IMG_FLECHE_AIDE_ANDROID..Visible = faux
      	<sinon>
      		IMG_FLECHE_AIDE_ANDROID..Visible = bAideVisible
      		IMG_FLECHE_AIDE_IOS..Visible = faux
      	<FIN>
      		
      	// Lancement terminé
      	Lancement = faux
     type : 458752
