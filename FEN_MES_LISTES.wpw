   -
     name : AidePremièreListe
     procedure_id : 1609461504563573217
     type_code : 14
     code : |1-
      // Résumé : Aide à la première création de liste
      // Syntaxe :
      // AidePremièreListe ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AidePremièreListe()
      
      	<compile si TypeConfiguration =Android>
      		si pas BTN_AJOUTER_LISTE..Visible alors BTN_AJOUTER_LISTE..Visible = vrai
      	<FIN>
      
      	// Il faut au moins une liste
      	SI ZR_LISTES..Occurrence > 0 ALORS
      	
      		// Première création
      		SI EnModeTest _OU_ ChargeParamètre(PARAM_FIRSTHELP_LISTE, "") = "" ALORS
      			SauveParamètre(PARAM_FIRSTHELP_LISTE, "OK")
      			
      			// Un seul item dans la liste
      			SI ZR_LISTES..Occurrence = 1 ALORS
      				
      				// On affiche l'aide en dessous
      				LIB_INFO_FIRST_CREATION..Y = ZR_LISTES..Y + ZR_LISTES..HauteurLigne + 1
      				
      				// Moins de 4 items dans la liste
      			SINON SI ZR_LISTES..Occurrence < 4
      				
      				// On affiche l'aide au dessus de l'item ajouté
      				LIB_INFO_FIRST_CREATION..Y = ZR_LISTES..Y + (ZR_LISTES..HauteurLigne * ZR_LISTES..Occurrence) + 1
      				
      				// Moins de 6 items dans la liste
      			SINON SI ZR_LISTES..Occurrence < 6
      				
      				// On affiche l'aide au dessus de l'item ajouté
      				LIB_INFO_FIRST_CREATION..Y = ZR_LISTES..Y + (ZR_LISTES..HauteurLigne * (ZR_LISTES..Occurrence - 2)) + 1
      				
      				<COMPILE SI MODE_ANDROID>
      					LIB_INFO_FIRST_CREATION..CadrageVertical = cvBas					
      				<FIN>
      				
      			SINON
      				
      				// Autrement, on affiche l'aide en première ligne
      				LIB_INFO_FIRST_CREATION..Y = ZR_LISTES..Y
      				
      				<COMPILE SI MODE_ANDROID>
      					LIB_INFO_FIRST_CREATION..CadrageVertical = cvBas
      				<FIN>
      			FIN
      			
      			LIB_INFO_FIRST_CREATION..Visible = Vrai			
      			CacheFirstHelp_Liste_Différé()
      		FIN
      	fin
