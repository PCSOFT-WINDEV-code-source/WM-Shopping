      -
        internal_properties : CAAAAAgAAABfNBISf12SPxBMKc9oDEIdQhQWJKvHsF3ZOgyKJnFPkYClLe1MS/m/IiGK082EUTRbV6GH9F26twAhVTb/b3fcGXXhmTDH4Y99vprvGadWfRfU/Qfp30Hwebr30e83HuAlSS6cdn+bytqsfDueT2VZlLs5H9lkiZqBvib/pRPQUw==
        code : |1-
         
         // On est sur le plan des modifications
         SI MaFenêtre..Plan = 2 ALORS
         	
         	<COMPILE SI MODE_ANDROID>
         		AnimationPrépare(MaFenêtre)
         	<FIN>
         	
         	// Revient sur le plan 1
         	MaFenêtre..Plan = 1
         		
         	OPT_TERMINER..Visible = faux
         	
         	// Option visible en IOS/Android
         	OPT_CREER_LISTE..Visible = EnModeiOS()
         	BTN_AJOUTER_LISTE..Visible = EnModeAndroid() 
         	
         	// Modifie l'image de l'action-bar
         	SI ACTB_MAIN..Image <> IMG_MENU ALORS
         		ACTB_MAIN..Image = IMG_MENU			
         	FIN
         	
         	<COMPILE SI MODE_ANDROID>
         		Animationjoue(animFondu)
         	<FIN>
         	
         	RETOUR
         FIN
         
         // Si le sliding menu est ouvert
         SI FenCoulissanteVisible(fcGauche) ALORS
         	
         	// Ferme le sliding menu
         	FenCoulissanteVisible(fcGauche, Faux)		
         	RETOUR
         FIN
         
         // Si un balayage est en cours sur la zone répétée
         si ListeManager.BalayageEnCours ALORS
         
         	// Annule la balayage en cours
         	ZoneRépétéeAnnuleBalayage(ZR_LISTES)
         	retour
         FIN
         
         // Si une liste est ouverte
         // On ferme cette liste
         si ListeManager.FermeTout() ALORS
         	
         	// Et si une liste était encore ouverte, 
         	// on annule la fermeture de la fenêtre principale
         	retour
         FIN
         	
         ferme
