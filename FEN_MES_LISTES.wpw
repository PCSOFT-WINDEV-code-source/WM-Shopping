      p_codes :
       -
         code : |1-
          
          // Enregistre les nouvelles positions
          nIndice est un entier
          
          // Réordonne
          POUR TOUTE LIGNE nIndice DE ZR_LISTES_TRI
          	
          	HLitRecherchePremier(Liste, IDListe, ZR_LISTES_TRI.ATT_ID)
          	SI HTrouve(Liste) ALORS
          		
          		// Ré-écrit la position
          		Liste.Position = nIndice
          		HModifie(Liste)
          	FIN	
          FIN
          
          <COMPILE SI MODE_ANDROID>
