  p_codes :
   -
     code : |1-
      PROCEDURE MaFenêtre(Rayons est un tableau de STRayon, CatégorieSélectionnée est une chaine)
      
      	SourceRecherche est un tableau de chaines
      	CurseurHolo est un entier = 1
      
      local
      	stUnRayon est un STRayon
      	nUnRayon est un entier
      	nHauteur est un entier
      	nRayon est un entier
      	
      	
      	// Remplissage
      	pour tout stUnRayon de Rayons
      		
      		// Ajoute une ligne
      		nUnRayon = ZoneRépétéeAjouteLigne(ZR_RAYONS, stUnRayon.GUID, stUnRayon.Nom)
      		
      		// Cumule la hauteur
      		nHauteur += ZR_RAYONS..Hauteurligne
      		
      		// Met en évidence la catégorie actuellement sélectionnée
      		si stUnRayon.GUID = CatégorieSélectionnée ALORS
      			ZR_RAYONS[nUnRayon].LIB_RAYON..PoliceGras = vrai
      			nRayon = nUnRayon
      		FIN
      		
      		// Conserve le nom du rayon pour la recherche
      		ajoute(SourceRecherche, ChaîneFormate(stUnRayon.Nom, ccSansPonctuationNiEspace + ccMinuscule + ccSansAccent))
      	FIN
      	
      	MaFenêtre..ValeurRenvoyée = ""
      
      	// Positionne l'ascenseur à l'emplacement du rayon
      	ZoneRépétéePosition(ZR_RAYONS, max(1, nRayon - 1))	
