     name : FinRecherche
     procedure_id : 1562198773746547538
     type_code : 14
     group : 2
     code : |1-
      PROCEDURE FinRecherche(VOID <utile> = null)
      	
      	// Si la recherche n'est pas active
      	<COMPILE SI MODE_ANDROID>
      		si OPT_RECHERCHE_AJOUTER..Visible ALORS retour
      	<sinon>
      		SI BTN_AJOUTER_PRODUIT_IOS..etat = actif ALORS RETOUR	
      	<fin>
      	
      	// Cache la recherche
      	ActionBarRechercheVisible(Faux)
      	
      	<COMPILE SI MODE_ANDROID>
      		OPT_RECHERCHE_AJOUTER..Visible = Vrai
      	<sinon>
      		BTN_AJOUTER_PRODUIT_IOS..CouleurFond = IMG_NAVBAR_IOS..CouleurFond
      		BTN_AJOUTER_PRODUIT_IOS..Opacité = 100
      		BTN_AJOUTER_PRODUIT_IOS..etat = actif
      	<FIN>
      	
      	nPlan est un entier = CFI_ZAB_CONTENU..Note
      	
      	// Si on est sur la liste des produits + historique
      	SI FIListePosition(CFI_ZAB_CONTENU) - nPlan = 1 ALORS
      		
      		// On passe au contenu de la liste
      		FIListePrécédent(CFI_ZAB_CONTENU)		
      	sinon
      		
      		FIListePosition(CFI_ZAB_CONTENU, nPlan)
      	FIN
      
      	ModifieIconeMenuSelonPlan()
