  internal_properties : CAAAAAgAAAArFMLlpoDCWvPbdj1zCMWdYTPUfKXXzMuNGUhLxa8O1HpA9hFNXIPh0Z7mWsY9tGZqjvtgqRXWJlDHL6Q0DURY3cj18rA41RaxnYQ79wbiRihrrlEviZXnA3r6vgzKTpHnHWYR
  type_code : 1
  p_codes :
   -
     code : |1-
      PROCEDURE MaFenêtre(MonAlias est une chaine, IDListe est un entier sur 8)
      
      MaFenêtre..Alias = monalias
      
      MaListe est une CListeShopping dynamique = CListeShopping::Charge(idliste, monalias)
      ThemeCouleur est un entier = ACTB_MAIN..CouleurFond
      
      // Nom de la liste en titre de fenêtre
      mafenetre..titre = maliste.Nom
      
      // Effacement automatique : à faire automatiquement avant la déclaration des 
      // observateurs pour éviter les mises à jour superflues
      SI USER_PARAMS.EffacerProduitsAchetésAuto ALORS
      	
      	// Si on n'a pas encore nettoyé la liste
      	SI ListeManager.ListeNettoyée[IDListe]..Vide ALORS
      		
      		// Recherche si il y a au moins un produit acheté dans la liste
      		HRecherchePremier(ListeContenu, AchetéDansListe, [idliste, Vrai])
      		si htrouve(ListeContenu) ALORS
      			
      			// Supprime les produits achetés
      			MaListe.SupprimeTousProduits(Vrai)
      			ListeManager.ListeNettoyée[IDListe] = vrai
      			
      			ToastAffiche(<§Rff0007af112a68c2¤fr-FR¤Les produits achetés ont été placés dans l'historique§>, toastLong)
      		sinon
      			ListeManager.ListeNettoyée[IDListe] = Faux
      		FIN		
      	FIN		
      sinon
      	// Marquage pour éviter un effacement à la prochaine ouverture
      	// si l'option d'effacement automatique a été modifiée entre-temps
      	ListeManager.ListeNettoyée[IDListe] = faux
      FIN
      
      // Observateurs
      AjouteObservateur(MaListe.SurListeModifiée, MonAlias, SurListeModifiée)
      AjouteObservateur(MaListe.SurListeMiseAJour, MonAlias, SurListeMiseAJour)
      AjouteObservateur(ListeManager.SurDemandeFinRecherche, MonAlias, FinRecherche)
      
      // Liste originale
      CFI_ZAB_CONTENU..BalayageActif = faux
      ACTB_MAIN..image = IMG_MENU
      
      // Mise à jour
      Actualise()
      
      FIListeAjoute(CFI_ZAB_CONTENU, FI_CONTENU_LISTE, MaListe, ThemeCouleur)
      FIListeAjoute(CFI_ZAB_CONTENU, FI_CONTENU_HISTORIQUE, MaListe, ThemeCouleur)
