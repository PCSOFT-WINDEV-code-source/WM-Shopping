      PROCEDURE SurListeMiseAJour(VOID <utile> = null)
      	
      	bAvancé est un booléen
      	bProduitsAchetés est un booléen = (MaListe.ProduitsAchetésOccurrence > 0)
      	bListeNonVide est un booléen = (MaListe.ProduitsOccurrence + MaListe.ProduitsAchetésOccurrence > 0)
      	
      	// Menus
      	si OPT_VIDER_LISTE..Visible <> bListeNonVide 			alors OPT_VIDER_LISTE..Visible 			= bListeNonVide
      	si OPT_ENVOI_MAILSMS..Visible <> bListeNonVide 			alors OPT_ENVOI_MAILSMS..Visible		= bListeNonVide
      	si OPT_SUPPRIMER_ACHETES..Visible <> bProduitsAchetés 	alors OPT_SUPPRIMER_ACHETES..Visible 	= bProduitsAchetés
      	
      	bAvancé = (bProduitsAchetés _ou_ bListeNonVide)
      		
      	// Sous IOS, cache les options de menu avancé
      	// -> elle seront accessibles par le menu principal
      	<COMPILE SI mode_ios>
      		
      		si OPT_SUPPRIMER_ACHETES1..Visible 	alors OPT_SUPPRIMER_ACHETES1..Visible = Faux
      		si OPT_VIDER_LISTE1..Visible 		alors OPT_VIDER_LISTE1..Visible = Faux
      		si OPT_ENVOI_MAILSMS1..Visible		alors OPT_ENVOI_MAILSMS1..Visible = Faux
      		
      		si OPT_HISTORIQUE1..Visible 		alors OPT_HISTORIQUE1..Visible = faux
      		si OPT_ORDRE_RAYON..Visible 		alors OPT_ORDRE_RAYON..Visible = Faux
      		si OPT_ORDRE_RAYON1..Visible 		alors OPT_ORDRE_RAYON1..Visible = Faux
      		si OPT_AVANCES..Visible <> bAvancé	alors OPT_AVANCES..Visible = bAvancé
      	<SINON>
      		
      		SI OPT_VIDER_LISTE1..Visible <> bListeNonVide 			ALORS OPT_VIDER_LISTE1..Visible 		= bListeNonVide
      		SI OPT_ENVOI_MAILSMS1..Visible <> bListeNonVide 		ALORS OPT_ENVOI_MAILSMS1..Visible 		= bListeNonVide
      		SI OPT_SUPPRIMER_ACHETES1..Visible <> bListeNonVide 	ALORS OPT_SUPPRIMER_ACHETES1..Visible 	= bProduitsAchetés
      		
      		si OPT_HISTORIQUE1..Visible = faux 		alors OPT_HISTORIQUE1..Visible = Vrai
      		SI OPT_ORDRE_RAYON..Visible = Faux 		ALORS OPT_ORDRE_RAYON..Visible = Vrai
      		SI OPT_ORDRE_RAYON1..Visible = Faux 	ALORS OPT_ORDRE_RAYON1..Visible = Vrai
      		si OPT_AVANCES..Visible 				alors OPT_AVANCES..Visible = Faux
      	<FIN>
      
      	// Nom de la liste en titre de fenêtre
      	sTitre est une chaine = chaineconstruit("%1 (%2)", MaListe.Nom, MaListe.ProduitsQuantitéTotale)
      	si FEN_LISTE..Titre <> sTitre alors FEN_LISTE..Titre = sTitre
      
      	// Mise à jour
      	ListeManager.DéclencheListesActualisées(faux, vrai, (maliste.IDListe))
     type : 458752
