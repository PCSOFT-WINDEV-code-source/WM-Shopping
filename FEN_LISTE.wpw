     name : SurSaisieRecherche
     procedure_id : 1617585143244310060
     type_code : 14
     code : |1+
      // Résumé : Lors de la saisie de la recherche...
      // Syntaxe :
      // SurSaisieRecherche ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SurSaisieRecherche()
      
      	// Si on est sur l'historique
      	SI FIListePosition(CFI_ZAB_CONTENU) = 2 _ET_ PAS ACTB_MAIN..ValeurRecherche ~= "" ALORS
      		
      		// Conserve l'écran
      		CFI_ZAB_CONTENU..Note = FIListePosition(CFI_ZAB_CONTENU)
      		
      		// On passe à la recherche	
      		FIListeSuivant(CFI_ZAB_CONTENU)	
      		ActionBarRechercheVisible(Vrai)
      		
      	SINON SI FIListePosition(CFI_ZAB_CONTENU) = 1 ALORS
      		
      		// Conserve l'écran
      		CFI_ZAB_CONTENU..Note = FIListePosition(CFI_ZAB_CONTENU)
      		
      		// Recherche
      		FIListePosition(CFI_ZAB_CONTENU, 3)	
      		ActionBarRechercheVisible(Vrai)
      	FIN
      	
      	// Exécute la recherche
      	ListeManager.DéclencheRecherche(ACTB_MAIN..ValeurRecherche)
      	
      	// Pas de recherche ?
      	SI ACTB_MAIN..ValeurRecherche ~= "" ALORS
      		
      		// Si on est positionné sur le plan de recherche
      		SI FIListePosition(CFI_ZAB_CONTENU) = 3 ALORS
      			
      			// On a des entrées d'historique
      			SI ListeManager.NbHistorique > 0 ALORS
      				
      				// On passe à l'historique
      				FIListePosition(CFI_ZAB_CONTENU, 2)		
      			SINON
      				
      				// On repasse à l'écran précédent
      				FIListePosition(CFI_ZAB_CONTENU, Val(CFI_ZAB_CONTENU..Note))		
      			FIN
      			
      			ActionBarRechercheVisible(Vrai)				
      		FIN
      	FIN
     type : 458752
