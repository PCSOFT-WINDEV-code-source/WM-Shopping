     code : |1-
      PROCEDURE ModifieIconeMenuSelonPlan()
      
      	SELON FIListePosition(CFI_ZAB_CONTENU)
      		
      		// Contenu de la liste
      		CAS 1
      			
      			<COMPILE SI MODE_android>
      				
      				SI ACTB_MAIN..Image <> IMG_MENU ALORS
      					ACTB_MAIN..Image = IMG_MENU				
      				FIN
      				
      			<sinon>
      			
      				// Si le sliding menu est ouvert
      				SI FenCoulissanteVisible(fcGauche) ALORS
      					
      					// On reste avec l'image du menu
      					SI ACTB_MAIN..Image <> IMG_MENU ALORS
      						ACTB_MAIN..Image = IMG_MENU				
      					FIN
      				SINON
      					
      					// On reste avec l'image de retour
      					SI ACTB_MAIN..Image <> IMG_RETOUR ALORS
      						ACTB_MAIN..Image = IMG_RETOUR
      					FIN
      				FIN
      				
      			<FIN>			
      						
      			// Nom de la liste en titre de fenêtre
      			sTitre est une chaine = chaineconstruit("%1 (%2)", MaListe.Nom, MaListe.ProduitsQuantitéTotale)
      			SI FEN_LISTE..Titre <> sTitre ALORS FEN_LISTE..Titre = sTitre
      			
      			<COMPILE SI MODE_IOS>
      				
      				SI BTN_HISTORIQUE_IOS..Etat = Inactif ALORS
      					BTN_HISTORIQUE_IOS..CouleurFond = IMG_NAVBAR_IOS..CouleurFond
      					BTN_HISTORIQUE_IOS..Opacité = 100
      					BTN_HISTORIQUE_IOS..Etat = Actif
      				FIN
      				
      				BTN_MA_LISTE..CouleurFond = 0x101010
      				BTN_MA_LISTE..Opacité = 60
      				BTN_MA_LISTE..Etat = Inactif
      			<FIN>
      									
      		// Historique
      		CAS 2
      			SI ACTB_MAIN..Image <> IMG_RETOUR ALORS
      				ACTB_MAIN..Image = IMG_RETOUR
      			fin
      			
      			<compile SI MODE_IOS>
      				BTN_HISTORIQUE_IOS..CouleurFond = 0x101010
      				BTN_HISTORIQUE_IOS..Opacité = 60
      				BTN_HISTORIQUE_IOS..etat = inactif
      				
      				SI BTN_MA_LISTE..Etat = Inactif ALORS
      					BTN_MA_LISTE..CouleurFond = IMG_NAVBAR_IOS..CouleurFond
      					BTN_MA_LISTE..Opacité = 100
      					BTN_MA_LISTE..Etat = Actif
      				fin
      				
      				OPT_AVANCES..Visible = Faux
      			<FIN>
      			
      			FEN_LISTE..Titre = <§Rff0002af112c68c2¤fr-FR¤Historique§>
      			
      			
      		// Recherche
      		CAS 3	
      			SI ACTB_MAIN..Image <> IMG_RETOUR ALORS
      				ACTB_MAIN..Image = IMG_RETOUR
      			FIN
      			
      			<COMPILE SI MODE_IOS>
      				si BTN_HISTORIQUE_IOS..etat = inactif alors
      					BTN_HISTORIQUE_IOS..CouleurFond = IMG_NAVBAR_IOS..CouleurFond
      					BTN_HISTORIQUE_IOS..Opacité = 100
      					BTN_HISTORIQUE_IOS..Etat = actif
      				fin
      				
      				SI BTN_MA_LISTE..Etat = Inactif ALORS
      					BTN_MA_LISTE..CouleurFond = IMG_NAVBAR_IOS..CouleurFond
      					BTN_MA_LISTE..Opacité = 100
      					BTN_MA_LISTE..Etat = Actif
      				FIN
      			<FIN>
      			
      			FEN_LISTE..Titre = <§Rff0003af112e68c2¤fr-FR¤Recherche§>
      	FIN
     type : 458752
