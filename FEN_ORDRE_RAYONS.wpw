   -
     name : TriOrdreRayon
     procedure_id : 1587851183191363239
     type_code : 14
     code : |1-
      PROCEDURE TriOrdreRayon(stRayon1 est une STRayon, stRayon2 est une STRayon)
      
      	nPos1 est un entier
      	nPos2 est un entier
      	
      	// Produit hors-rayon
      	SI stRayon1.Nom = "" ALORS nPos1 = 999999998
      	SI stRayon2.Nom = "" ALORS nPos2 = 999999998
      	
      	// Si un ordre des rayons a été fourni
      	SI gpclUneListe.OrdreRayon..Occurrence > 0 _ET_ nPos1 = 0 ALORS nPos1 = Cherche(gpclUneListe.OrdreRayon, tcLinéaire, stRayon1.GUID)
      	SI gpclUneListe.OrdreRayon..Occurrence > 0 _ET_ nPos2 = 0 ALORS nPos2 = Cherche(gpclUneListe.OrdreRayon, tcLinéaire, stRayon2.GUID)
      	
      	// Selon la position
      	SI nPos1 > nPos2 ALORS RENVOYER 1
      	SI nPos1 < nPos2 ALORS RENVOYER -1		
      	
      	// En dernier recours, ordre alphabétique de rayons
      	RENVOYER ChaîneCompare(stRayon1.Nom, stRayon2.Nom, ccSansCasse)
     type : 458752
   -
     name : ActualiseRayons
     procedure_id : 1588930233623767535
     type_code : 14
     code : |1-
      // Résumé : Actualise les rayons
      // Syntaxe :
      //ActualiseRayons ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ActualiseRayons()
      	
      	tabRayons est un tableau de STRayon
      	stCat est un STRayon
      	nLigne est un entier
      	
      	ZoneRépétéeSupprimeTout(ZR_RESULTATS)
      	
      	// Duplique les rayons racines
      	pour toute stCat de MesRayons
      		si stCat.GUIDParent = "" ALORS
      			Ajoute(tabRayons, stCat)
      		FIN
      	FIN
      	
      	// Tri effectif selon l'ordre établi pour la liste en cours
      	Trie(tabRayons, ttFonction, TriOrdreRayon)
      		
      	POUR TOUT stCat DE tabRayons
      		
      		nLigne = ZoneRépétéeAjouteLigne(ZR_RESULTATS)
      		ZR_RESULTATS[nLigne].ATT_ID 	= stCat.GUID
      		ZR_RESULTATS[nLigne].ATT_NOM 	= stCat.Nom
      		ZR_RESULTATS[nLigne].ATT_INDICE	= nLigne
      	FIN
     type : 458752
   -
     name : ValideListe
     procedure_id : 1614563788235067128
     type_code : 14
     code : |1+
      // Résumé : Valide la liste
      // Syntaxe :
      // ValideListe ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ValideListe()
      	
      	tabOrdre est un tableau de chaines
      	bOrdreModifié est un booléen
      	nLigne est un entier
      	
      	POUR TOUTE LIGNE nLigne DE ZR_RESULTATS
      		Ajoute(tabOrdre, ATT_ID)
      		
      		// Détermine si l'ordre a été modifié
      		SI nLigne <> ATT_INDICE[nLigne] ALORS bOrdreModifié = Vrai
      	FIN
      	
      	// Si l'ordre a été modifié
      	SI bOrdreModifié ALORS
      		
      		// Modifie l'ordre des rayons
      		gpclUneListe.SetOrdreRayons(tabOrdre, Vrai)
      	FIN
      	
      	MaFenêtre..ValeurRenvoyée = bOrdreModifié
      	Ferme(FEN_ORDRE_RAYONS)
     type : 458752
