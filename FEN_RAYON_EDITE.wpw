   -
     code : |1-
      PROCEDURE MaFenêtre(local RayonSélectionné est une chaine, LOCAL gsGUIDRayon est une chaine = "")
      
      	EstNouveau est un booléen
      	
      	MaFenêtre..ValeurRenvoyée = ""
      	
      	// Nouveau rayon ?
      	si gsGUIDRayon = "" ALORS
      		
      		// On crée une entrée
      		HRAZ(DonnéesPerso)
      		EstNouveau = vrai
      	sinon
      		
      		// On positionne sur le rayon
      		HLitRecherchePremier(DonnéesPerso, GUID, gsGUIDRayon)
      		si pâs htrouve(DonnéesPerso) ALORS
      			ToastAffiche("Rayon introuvable")
      			ferme
      		FIN
      		
      		// Parent
      		RayonSélectionné = DonnéesPerso.GUIDParent
      		EstNouveau = faux
      		
      		// Libellé du rayon
      		SAI_LIBELLE = DonnéesPerso.Nom
      	FIN
   -
     code : |1-
      
      	// Attribue le rayon par défaut
      	COMBO_RAYON = RayonSélectionné
      	ecranpremier(SAI_LIBELLE)
