  procedures :
   -
     name : ValideRayon
     procedure_id : 1616768923337096281
     type_code : 14
     code : |1-
      // Résumé : Valide l'édition du rayon
      // Syntaxe :
      //ValideRayon ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ValideRayon()
      
      	// Vérifie le nom
      	si SAI_LIBELLE ~= "" ALORS
      		ToastAffiche(<§Rff0001af10fe68c2¤fr-FR¤Veuillez saisir un nom§>)
      		DonneFocus(SAI_LIBELLE)
      		retour
      	FIN
      	
      	sIndex est une chaine = ChaîneVersIndex(SAI_LIBELLE)
      	sGUIDRayon est une chaine = COMBO_RAYON..ValeurMémorisée
      	
      	// Vérifie qu'un autre rayon ne porte pas déjà ce nom dans le même niveau de hiérarchie
      	nIndice est un entier = cherche(MesRayons, tcLinéairepremier, "NomIndex", sIndex)
      	tantque nIndice > 0
      		
      		// Même niveau
      		si MesRayons[nIndice].GUIDParent = sGUIDRayon ALORS
      			
      			// En création ou, en édition si le GUID est différent
      			si EstNouveau _ou_ MesRayons[nIndice].GUID <> DonnéesPerso.GUID ALORS
      				ToastAffiche(<§Rff0002af110068c2¤fr-FR¤Ce nom est déjà utilisé dans ce niveau de rayon§>)
      				DonneFocus(SAI_LIBELLE)
      				RETOUR
      			FIN
      		FIN
      		
      		// Recherche suivante
      		nIndice = cherche(MesRayons, tcLinéaireSuivant, "NomIndex", sIndex)
      	FIN
      	
      	// Mise à jour
      	DonnéesPerso.Nom		= sai_libelle	
      	DonnéesPerso.NomIndex	= sIndex
      	DonnéesPerso.GUIDParent = sGUIDRayon
      	DonnéesPerso.Code 		= ""
      	DonnéesPerso.Image 	 	= ""
      	DonnéesPerso.Produit	= faux
      	DonnéesPerso.Langue	 	= nation
      	
      	si EstNouveau alors
      		DonnéesPerso.GUID = DonneGUID(guidBrut)
      	fin
      	
      	// Enregistre
      	HEnregistre(DonnéesPerso)
      	sGUIDRayon = DonnéesPerso.GUID
      			
      
      	TitreSuivant(<§Rff0003af110268c2¤fr-FR¤Veuillez patienter...§>)
      	sablier(vrai)
      	
      	// Mise à jour des sources de données
      	SourceActualise(Vrai)
      	
      	// Mise à jour des rayons effectuée
      	ListeManager.DéclencheRayonsModifiés()
      	
      	sablier(faux)
      	TitreSuivant("")
      	
      	// On renvoie le GUID
      	mafenetre..ValeurRenvoyée = sGUIDRayon
      	
      	// Ferme
      	ferme()
