   -
     code : |1-
      PROCEDURE MaFenêtre()
      
      gbOriginalInversionGaucheDroite est un booléen = USER_PARAMS.InversionGaucheDroite
      gbOriginalEffacerProduitsAchetésAuto est un booléen = USER_PARAMS.EffacerProduitsAchetésAuto
      gbOriginalVerrouillerEcran est un booléen = USER_PARAMS.VerrouillerEcran
      
      INT_COCHE_INVERSEE = USER_PARAMS.InversionGaucheDroite
      INT_EFFACER_AUTO = USER_PARAMS.EffacerProduitsAchetésAuto
      INT_VERROUILLER = USER_PARAMS.VerrouillerEcran
      
      ExécuteTraitement(INT_COCHE_INVERSEE, trtModification)
      ExécuteTraitement(INT_EFFACER_AUTO, trtModification)
      ExécuteTraitement(INT_VERROUILLER, trtModification)
   -
     code : |1+
      
      
     type : 234
   -
     code : |1+
      
      
     type : 165
   -
     code : |1+
      
      
     type : 177
   -
     type : 180
     enabled : false
   -
     type : 230
     enabled : false
   -
     code : |1-
      
     type : 40
   -
     code : |1-
      
      // Sauvegarde des paramètres
      USER_PARAMS.InversionGaucheDroite = INT_COCHE_INVERSEE
      USER_PARAMS.EffacerProduitsAchetésAuto = INT_EFFACER_AUTO
      USER_PARAMS.VerrouillerEcran = INT_VERROUILLER
      
      // Détermine si les paramètres on été modifiés
      bModifié est un booléen = gbOriginalInversionGaucheDroite <> USER_PARAMS.InversionGaucheDroite _ou_ ...
      						  gbOriginalEffacerProduitsAchetésAuto <> USER_PARAMS.EffacerProduitsAchetésAuto _ou_ 
      						  gbOriginalVerrouillerEcran <> USER_PARAMS.VerrouillerEcran
      
      // Si les paramètres ont été modifiés
      si bModifié alors
      	
      	// En réel
      	SI EnModeSimulateur = Faux ALORS
      	
      		// Cas du verrouillage modifié
      		si gbOriginalVerrouillerEcran <> USER_PARAMS.VerrouillerEcran ALORS
      			
      			// Si l'écran doit être verrouillé
      			SI USER_PARAMS.VerrouillerEcran ALORS
      				
      				// Désactive la mise en veille
      				SysMiseEnVeille(sysBatterie, sysVeilleInactive)
      				
      			// Si l'écran ne doit pas être verrouillé
      			sinon
      				
      				// Active la mise en veille
      				SysMiseEnVeille(sysBatterie, sysVeilleactive)
      			FIN		
      		FIN
      	fin
      	
      	// Sauvegarde effective
      	SauveParamètre(PARAM_COCHE_INVERSEE, USER_PARAMS.InversionGaucheDroite)
      	SauveParamètre(PARAM_EFFACER_AUTO, USER_PARAMS.EffacerProduitsAchetésAuto)
      	SauveParamètre(PARAM_VERROUILLER_ECRAN, USER_PARAMS.VerrouillerEcran)
      
      	// Préviens l'application que les paramètres ont été modifiés
      	ListeManager.DéclencheParamètresModifiés()
      fin
