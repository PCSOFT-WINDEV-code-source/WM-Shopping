    code_elements :
     type_code : 126
     p_codes :
      -
        code : |1-
         
         // Selon le paramètre cliqué
         selon ZM_PARAMETRES
         			
         	// Coche inversée
         	cas 2
         		
         		INT_COCHE_INVERSEE = pas INT_COCHE_INVERSEE
         		ExécuteTraitement(INT_COCHE_INVERSEE, trtModification)
         			
         	// Coche inversée
         	cas 3
         		
         		INT_EFFACER_AUTO = pas INT_EFFACER_AUTO
         		ExécuteTraitement(INT_EFFACER_AUTO, trtModification)
         		
         	// Verrouillage
         	cas 4
         		INT_VERROUILLER = pas INT_VERROUILLER
         		ExécuteTraitement(INT_VERROUILLER, trtModification)
         		
         	/////////
         	
         	// Réinitialiser
         	cas 7
         		
         		//1 : &Réinitialiser
         		//2 : &Annuler
         		SELON Dialogue(<§?160B94B01E573F42§>)
         			// &Réinitialiser
         			CAS 1
         				
         				sablier(vrai)
         				
         				//
         				// --- Suppression des données HFSQL				
         				//
         				
         				// Libère les requêtes
         				HLibèreRequête(REQ_HISTORIQUE_CONTENU)
         				HLibèreRequête(REQ_LISTE_CONTENU)
         				HLibèreRequête(REQ_LISTES)
         				HLibèreRequête(REQ_LISTES_NBELEMENTS)
         				HLibèreRequête(REQ_RAYONS_LISTING)
         				HLibèreRequête(REQ_RECHERCHE)
         				HLibèreRequête(REQ_RECHERCHE_RAYON)
         				HLibèreRequête(REQ_UNITES)
         				HLibèreRequête(REQ_SOURCES_LISTE)
         				HLibèreRequête(REQ_RAYONS)
         				HLibèreRequête(REQ_PRODUITS)
         				HLibèreRequête(REQ_SOURCE_LISTECONTENU)
         				
         				// Ferme les fichiers utilisateurs
         				HAnnuleAlias(DonnéesPerso)
         				hferme("*")
         
         				// RAZ des favoris
         				supprimetout(MesProduitsFavoris)
         				SauveParamètre(PARAM_USER_FAVORITES, "")
         				
         				//
         				// --- Suppression des fichiers "annexes"
         				//
         				
         				si pas EnModeSimulateur alors
         				
         					// Supprime les images des produits
         					fsupprime(DATA_IMAGE_PRODUCTS + [fsep] + "*.*")
         					
         					// Supprime les images des enseignes
         					fsupprime(DATA_IMAGE_SUPPLIER + [fsep] + "*.*")
         				
         					// Supprime les dossiers
         					frepsupprime(DATA_IMAGE_PRODUCTS)
         					frepsupprime(DATA_IMAGE_SUPPLIER)
         				fin
         				
         				// Réinitialise le stockage
         				InitialiseStockage()
         				
         				// Paramètres initiaux
         				SauveParamètre(PARAM_FIRSTHELP_LISTE, "")
         				
         				//
         				// --- Réamorçage
         				//
         				
         				// Mise à jour des sources de contenu
         				TitreSuivant(<§Rff0000af111068c2¤fr-FR¤Chargement des données...§>)
         				
         				si pas EnModeSimulateur alors
         										
         					// Extrait localement les fichiers de données de l'application
         					HFExtraitRessource(Données)
         					HFExtraitRessource(Fournisseur)
         					HFExtraitRessource(Unité)					
         				fin
         				
         				// RAZ les fichiers utilisateurs
         				HCréation(Historique)
         				HCréation(Liste)
         				HCréation(ListeContenu)
         				HCréation(Personnel)
         				HCréation(Note)
         												
         				// Ré-ouvre les fichiers
         				houvre(donnees)
         				houvre(Fournisseur)
         				houvre(Unité)
         				
         				// Crée un alias pour la gestion des rayons
         				HAlias(Personnel, DonnéesPerso)
         				HChangeNom(DonnéesPerso, Personnel..Nom)
         				HChangeRep(DonnéesPerso, DATA_PATH)
         				
         				// Mise à jour des sources de données
         				SourceActualise()
         				
         				// Ferme les listes ouvertes
         				ListeManager.FermeTout()
         				
         				// Actualise
         				ListeManager.DéclencheListesActualisées()				
         				
         				TitreSuivant("")
         				Sablier(faux)
         				
         				// Ferme
         				Ferme
         		FIN
         		
         FIN
