  procedures :
   -
     name : ActualiseContenu
     procedure_id : 1562177217310215622
     type_code : 14
     code : |1-
      PROCEDURE ActualiseContenu(LOCAL nIDItem est un entier sur 8 = -1)
      
      	nLigneSurbrillance est un entier
      	nProduitsAchetésOccurrence est un entier
      	nProduitsOccurrence est un entier
      	nProduitsQuantité est un entier
      	tabRayons est un tableau de chaines
      	tabOrdres est un tableau de chaines
      
      	// Liste invalide
      	si gpclUneListe = null alors renvoyer 0
      	si pas gpclUneListe.IDListe > 0 alors renvoyer 0
      
      	// Relit la liste
      	HLitRecherchePremier(liste, IDListe, gpclUneListe.IDListe)
      
      	// Mise à jour de l'ordre des rayons
      	ChaîneVersTableau(Liste.OrdreRayons, tabOrdres, ",")
      	gpclUneListe.SetOrdreRayons(tabOrdres)
      
      	// Vide la liste
      	ZoneRépétéeSupprimeTout(ZR_SHOPPING_LIST)
      
      	// Récupère le contenu de la liste
      	REQ_LISTE_CONTENU.pIDListe = gpclUneListe.IDListe
      	SI HExécuteRequête(REQ_LISTE_CONTENU) ALORS
      		
      		Items est un tableau de ListeElément
      		stElément est un ListeElément
      				
      		HLitPremier(REQ_LISTE_CONTENU)
      		tantque pas hendehors(REQ_LISTE_CONTENU)
      			
      			// Si le produit listé n'existe plus
      			si REQ_LISTE_CONTENU.GUIDOriginal dans (null, "") ALORS
      				// On doit le supprimer (sans notification)
      				xlog("[ActualiseContenu] Le produit " + REQ_LISTE_CONTENU.GUIDProduit + " n'existe plus !")
      				gpclUneListe.ProduitSupprime((REQ_LISTE_CONTENU.GUIDProduit), -1, faux, 0, vrai)
      				HLitSuivant(REQ_LISTE_CONTENU)
      				continuer
      			FIN
      		
      			stElément.ID = REQ_LISTE_CONTENU.IDListeContenu
      			stElément.GUIDRayon = REQ_LISTE_CONTENU.GUIDCatégorie
      			stElément.GUIDProduit = REQ_LISTE_CONTENU.GUIDProduit
      			stElément.UnitéID = REQ_LISTE_CONTENU.IDUnité
      			stElément.UnitéNom = REQ_LISTE_CONTENU.UnitéNom
      			stElément.UnitéNomPluriel = REQ_LISTE_CONTENU.UnitéNomPluriel
      			stElément.Acheté = REQ_LISTE_CONTENU.Acheté
      			stElément.Image = REQ_LISTE_CONTENU.Image
      			stElément.Note = remplace(REQ_LISTE_CONTENU.Note, "&", "&&")
      			stElément.Produit = Remplace(REQ_LISTE_CONTENU.NomProduit, "&", "&&")
      			stElément.Quantité = REQ_LISTE_CONTENU.Quantité
      			stElément.QuantitéVirtuelle = REQ_LISTE_CONTENU.QuantitéVirtuelle
      			stElément.DateAjout = REQ_LISTE_CONTENU.DateCréation
      			
      			// Changement de rayon
      			si stElément.achete ALORS
      				stElément.Rayon = <§Rff0000af10fa68c2¤fr-FR¤Produits achetés§>
      			sinon
      				stElément.Rayon = RayonLibellé(REQ_LISTE_CONTENU.GUIDCatégorie)
      			FIN
      						
      			ajoute(Items, stElément)			
      		
      			HLitSuivant(REQ_LISTE_CONTENU)
      		fin
      		
      		// Tri général
      		trie(Items, ttFonction, TriListeContenu)
      							
      		pour tout stElément de Items
      			
      			nCouleur est un entier = blanc
      			nCouleurTexte est un entier = 0x859300
      			nAlignTexte est un entier = cvbas
      			
      			// Conserve le rayon
      			SI PAS Cherche(tabRayons, tcLinéaire, stElément.Rayon) > 0 ALORS
      				Ajoute(tabRayons, stElément.Rayon)
      			FIN
      					
      			// Si le produit est marqué 'acheté'
      			SI stElément.Acheté ALORS
      				
      				// On force un rayon
      				nCouleur <=> nCouleurTexte
      				nAlignTexte = cvMilieu
      				nProduitsAchetésOccurrence ++
      			sinon
      				
      				// Unité de base (pièce)
      				si stElément.UnitéID dans (0, UNITEDEFAUT()) alors
      					// On comptabilise la quantité
      					nProduitsQuantité += (stElément.Quantité = 0 ? 1 sinon stElément.Quantité)
      				
      				// Autre unité (mesure)
      				sinon
      					// On comptabilise un produit
      					nProduitsQuantité += 1
      				fin
      				
      				// Entrées restantes
      				nProduitsOccurrence ++
      			FIN
      			
      			nLigne est un entier = ZoneRépétéeAjouteLigne(ZR_SHOPPING_LIST, stElément.GUIDRayon, stElément.ID, stElément.GUIDProduit)
      			
      			// Attribut d'achat
      			ZR_SHOPPING_LIST.ATT_ACHETE[nLigne]						= stElément.Acheté		
      			
      			// Modifie l'aspect de la ligne
      			ModifieAspectLigne(nLigne)
      						
      			ZR_SHOPPING_LIST[nLigne].LIB_RAYON						= stElément.Rayon
      			ZR_SHOPPING_LIST[nLigne].ATT_QUANTITE					= (stElément.Quantité = 0 ? 1 SINON stElément.Quantité)
      			ZR_SHOPPING_LIST[nLigne].ATT_CADRAGERAYON				= nAlignTexte
      			ZR_SHOPPING_LIST[nLigne].LIB_RAYON..Couleur 			= nCouleurTexte
      			ZR_SHOPPING_LIST[nLigne].LIB_RAYON..CouleurFond 		= nCouleur
      			ZR_SHOPPING_LIST[nLigne].IMG_FOND_RAYON..CouleurFond 	= nCouleur
      			ZR_SHOPPING_LIST[nLigne].LIB_PRODUIT 					= stElément.Produit
      			
      			si stElément.Image = "" _ou_ fFichierExiste(DATA_IMAGE_PRODUCTS + [fSep] + stElément.Image) = faux ALORS
      				ZR_SHOPPING_LIST[nLigne].IMG_PICTO_PRODUIT 			= IMG_PRODUIT
      			sinon
      				ZR_SHOPPING_LIST[nLigne].IMG_PICTO_PRODUIT 			= DATA_IMAGE_PRODUCTS + [fSep] + stElément.Image
      			FIN
      			
      			// Aucune quantité saisie
      			SI stElément.UnitéNom = "" _ET_ stElément.Note = "" ALORS				
      				ZR_SHOPPING_LIST[nLigne].LIB_QUANTITE..Visible = Faux
      				
      				// Change le cadrage vertical et la hauteur du libellé				
      				ZR_SHOPPING_LIST[nLigne].LIB_PRODUIT..Hauteur = IMG_PICTO_PRODUIT..hauteur
      				ZR_SHOPPING_LIST.ATT_CADRAGE[nLigne] = cvMilieu
      			SINON
      				ZR_SHOPPING_LIST[nLigne].LIB_QUANTITE = ""
      				ZR_SHOPPING_LIST[nLigne].LIB_QUANTITE..Visible = Vrai
      				
      				// Quantité
      				SI PAS stElément.UnitéNom = "" ALORS
      					ZR_SHOPPING_LIST[nLigne].LIB_QUANTITE = stElément.Quantité + [" "] + ...
      					(stElément.Quantité > 1 ? stElément.UnitéNomPluriel SINON stElément.UnitéNom)
      				FIN
      				
      				// Note 
      				ZR_SHOPPING_LIST[nLigne].LIB_QUANTITE += [" - "] + stElément.Note
      			FIN			
      			
      			// Mise en évidence de l'élément ajouté
      			si nIDItem > 0 _et_ stElément.ID = nIDItem ALORS
      				SurbrillanceLigne(nLigne, Surbrillance[1])
      				nLigneSurbrillance = nLigne
      			FIN
      		FIN
      	SINON
      		XLOG("[ActualiseContenu] (REQ_LISTE_CONTENU) Erreur = " + HErreurInfo)
      	FIN
      	
      	gpclUneListe.SetProduitsOccurrence(nProduitsOccurrence, nProduitsAchetésOccurrence, nProduitsQuantité)
      	gpclUneListe.SetRayons(tabRayons)
      	
      	si ZR_SHOPPING_LIST..Occurrence = 0 alors
      		
      		// Libellé vide + menu désactivé		
      		IMG_FLECHE_AIDE_IOS..Visible = EnModeiOS()
      		IMG_FLECHE_AIDE..Visible = EnModeAndroid()
      		LIB_AIDE..Visible = Vrai
      		LIB_INFO_LISTE_VIDE..Visible = Vrai
      				
      		ZR_SHOPPING_LIST..MenuContextuel = ""
      	sinon
      		GR_AIDE_CONTENUVIDE..Visible = faux
      		ZR_SHOPPING_LIST..MenuContextuel = MENU_CONTENU_LISTE_SUPPRESSION
      	FIN
      	
      	si EnModeSimulateur = faux alors SélectionAbandon = faux
      		
      renvoyer nLigneSurbrillance	
     type : 458752
