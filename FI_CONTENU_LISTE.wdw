     name : ListingPourExport
     internal_properties : CAAAAAgAAAA8h3jeRvHQwVZGWyWKOgs+QzBE/DIvmY5UruBFJUBSX5JH4cypNmQFHKT2iBbCFWrIcBTlFmJho8RwYf4cSGyRAzRKgiS+szIKop4GMBkKsdLPNdGJkmcEgO3oYieg0rv33v1P0NW5cqRXcpngBMXsec/bXmQ5POOcUTAa+Q3TQqp0ph6rGnlkCK0/3fCQSQEEprFMx8w=
     procedure_id : 1576018393646511458
     type_code : 14
     code : |1-
      PROCEDURE ListingPourExport(LOCAL bIgnoreProduitsAchetés est un booléen = vrai)
      
      	nLigne est un entier
      	sRayon est une chaine
      	sListe est une chaine
      		
      	Sablier(Vrai)
      	POUR TOUTE LIGNE nLigne DE ZR_SHOPPING_LIST
      		
      		// Ignore les produits achetés
      		SI bIgnoreProduitsAchetés _et_ ZR_SHOPPING_LIST[nLigne].ATT_ACHETE = Vrai ALORS CONTINUER
      		
      		// Si le rayon change
      		SI sRayon <> ZR_SHOPPING_LIST[nLigne].LIB_RAYON ALORS
      			
      			// Conserve le nouveau rayon et le marque dans l'email
      			sRayon = ZR_SHOPPING_LIST[nLigne].LIB_RAYON
      			sListe += [RC + RC] + ChaîneFormate(sRayon, ccMajuscule)
      		FIN
      		
      		// Complète le message avec le libellé du produit
      		sListe += [RC] + (["- "] + ZR_SHOPPING_LIST[nLigne].LIB_PRODUIT)
      		
      		// Si une quantité est indiquée (avec une éventuelle note)
      		SI ZR_SHOPPING_LIST[nLigne].LIB_QUANTITE..Visible ALORS
      			
      			// Complète le message avec la quantité
      			sListe += [" "] + (["("] + ZR_SHOPPING_LIST[nLigne].LIB_QUANTITE + [")"])
      		FIN
      	FIN
      	
      	Sablier(Faux)
      	
      renvoyer sListe
