   -
     name : ModifieAspectLigne
     procedure_id : 1585229505274596633
     type_code : 14
     code : |1-
      PROCEDURE ModifieAspectLigne(nLigne est un entier)
      	
      	nDiffLargDPI est un entier = (ZR_SHOPPING_LIST..Largeur - ZR_SHOPPING_LIST..LargeurInitiale)
      	
      	ZR_SHOPPING_LIST[nLigne].IMG_FOND_LIGNE..Etat 				= inactif
      	
      	SI ZR_SHOPPING_LIST.ATT_ACHETE[nLigne] ALORS
      		ZR_SHOPPING_LIST[nLigne].IMG_INT_LEFT = IMG_CHECK_ON
      		ZR_SHOPPING_LIST[nLigne].IMG_INT_RIGHT = IMG_CHECK_ON
      	SINON
      		ZR_SHOPPING_LIST[nLigne].IMG_INT_LEFT = IMG_CHECK_OFF
      		ZR_SHOPPING_LIST[nLigne].IMG_INT_RIGHT = IMG_CHECK_OFF			
      	FIN
      			
      	// Affiche l'interrupteur, image invisible
      	SI PAS USER_PARAMS.InversionGaucheDroite ALORS
      		ZR_SHOPPING_LIST[nLigne].IMG_INT_RIGHT..Visible 		= Vrai
      		ZR_SHOPPING_LIST[nLigne].IMG_INT_LEFT..Visible 			= Faux
      		ZR_SHOPPING_LIST[nLigne].IMG_PICTO_PRODUIT..X 			= IMG_PICTO_PRODUIT..XInitial
      		ZR_SHOPPING_LIST[nLigne].LIB_PRODUIT..X 				= LIB_PRODUIT..XInitial
      		ZR_SHOPPING_LIST[nLigne].LIB_QUANTITE..X				= LIB_QUANTITE..XInitial
      	SINON
      		ZR_SHOPPING_LIST[nLigne].IMG_INT_LEFT..Visible 			= Vrai
      		ZR_SHOPPING_LIST[nLigne].IMG_INT_RIGHT..Visible 		= Faux
      		ZR_SHOPPING_LIST[nLigne].IMG_PICTO_PRODUIT..X 			= IMG_PICTO_PRODUIT..XInitial + IMG_PICTO_PRODUIT..Largeur + 8
      		ZR_SHOPPING_LIST[nLigne].LIB_PRODUIT..X 				= LIB_PRODUIT..XInitial + IMG_PICTO_PRODUIT..Largeur + 8
      		ZR_SHOPPING_LIST[nLigne].LIB_QUANTITE..X				= LIB_QUANTITE..XInitial + IMG_PICTO_PRODUIT..Largeur + 8
      	FIN
      		
      	ZR_SHOPPING_LIST[nLigne].LIB_PRODUIT..Largeur 		= LIB_PRODUIT..LargeurInitiale + nDiffLargDPI
      	ZR_SHOPPING_LIST[nLigne].LIB_QUANTITE..Largeur 		= LIB_QUANTITE..LargeurInitiale + nDiffLargDPI
     type : 458752
