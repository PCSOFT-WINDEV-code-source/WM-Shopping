      PROCEDURE Actualise()
      	
      	// Lecture
      	HLitRecherchePremier(liste, idliste, idliste)
      	
      	// Affichage
      	FichierVersEcran
      	
      	// Visibilité
      	OPT_ENREGISTRER..Visible = enmodeios
