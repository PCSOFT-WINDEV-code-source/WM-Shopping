     name : SurRayonsModifiés
     procedure_id : 1616774721547638498
     type_code : 14
     code : |1-
      // Résumé : Mise à jour des rayons
      // Syntaxe :
      //SurRayonsModifiés ( [<VOID>])
      //
      // Paramètres :
      // 	VOID (valeur par défaut=0) : 
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SurRayonsModifiés(VOID <utile> = NULL)
      
      	sGUID est une chaine = COMBO_RAYON..ValeurMémorisée
      
      	// Vide les rayons et réactualise la liste
      	ListeSupprimeTout(COMBO_RAYON)
      	ExécuteTraitement(COMBO_RAYON, trtinit)
      	
      	// Re-sélectionne
      	COMBO_RAYON = sGUID
      	si ListeSelect(COMBO_RAYON) < 1 alors
      		
      		// Charge le produit original
      		HLitRecherchePremier(REQ_PRODUITS, GUID, gsGUID)
      				
      		COMBO_RAYON = REQ_PRODUITS.GUIDParent
      		gsGUIDParent = REQ_PRODUITS.GUIDParent
      		gbForceActualisation = vrai
      	FIN
     type : 458752
   -
     name : ActualiseLibelléUnité
     procedure_id : 1617571914576348861
     type_code : 14
     code : |1-
      // Résumé : Actualise la source du libellé des unités
      // Syntaxe :
      // ActualiseLibelléUnité ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ActualiseLibelléUnité()
      
      	nIDUnité est un entier = COMBO_UNITE
      	bUnitéModifiée est un booléen
      	
      	SI SAI_QUANTITE > 1 ALORS
      		SI COMBO_UNITE..RubriqueAffichée <> "NomPluriel" ALORS
      			COMBO_UNITE..RubriqueAffichée = "NomPluriel"		
      			bUnitéModifiée = Vrai
      		FIN
      	SINON
      		SI COMBO_UNITE..RubriqueAffichée <> "Nom" ALORS
      			COMBO_UNITE..RubriqueAffichée = "Nom"
      			bUnitéModifiée = Vrai
      		FIN
      	FIN
      	
      	SI bUnitéModifiée ALORS
      		COMBO_UNITE = nIDUnité
      	FIN
     type : 458752
   -
     name : ListeAjouteProduit
     procedure_id : 1619017373014422500
     type_code : 14
     code : |1-
      // Résumé : Ajoute le produit édité dans la liste spécifiée
      // Syntaxe :
      //ListeAjouteProduit (<sGUID> est chaîne, <pclListe> est CListeShopping dynamique)
      //
      // Paramètres :
      //	sGUID (chaîne UNICODE) :Liste
      //	pclListe (CListeShopping dynamique) :<indiquez ici le rôle de pclListe>
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ListeAjouteProduit(sGUID est une chaine, pclListe est un CListeShopping dynamique) 
      
      	
      	nIDUnité est un entier = COMBO_UNITE..ValeurMémorisée
      	
      	// RAZ
      	// Quantité nulle ou 1 pièce (unité par défaut)
      	SI SAI_QUANTITE = "" _OU_ (Val(SAI_QUANTITE) <= 1 _ET_ nIDUnité DANS (0, UNITEDEFAUT())) ALORS
      		
      		// N'affichera pas la quantité
      		nIDUnité = 0
      	FIN
      	
      	// Ajoute le produit créé à la liste
      	pclListe.ProduitAjoute(sGUID, (SAI_QUANTITE), "", "", (nIDUnité))
     type : 458752
