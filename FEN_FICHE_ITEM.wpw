         // Fiche modifiée
         si gbFicheModifiée _ou_ gbImageModifiée ALORS
         	
         	// On sollicite l'utilisateur
         	selon Dialogue(<§Rff000aaf115468c2¤fr-FR¤Les modifications n'ont pas été enregistrées.§>, [<§Rff000baf115268c2¤fr-FR¤Ignorer§>, <§Rff000caf115068c2¤fr-FR¤Annuler§>, <§Rff000daf114e68c2¤fr-FR¤Enregistrer§>], 3, 2)
         		
         		// Enregistrer
         		cas 3
         			
         			// Enregistre
         			ExécuteTraitement(BTN_VALIDER, trtclic)
         			retour
         		
         		// Ignorer
         		cas 1
         			// On ignore
         			si gbForceActualisation ALORS
         				MaFenêtre..ValeurRenvoyée = Vrai				
         			FIN
         			
         		// Annuler
         		cas 2	
         			retour		
         	FIN
         	
         FIN
         
         ferme
