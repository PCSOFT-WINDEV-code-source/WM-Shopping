   -
     name : OPT_ENLEVER_LISTEACHAT
     identifier : 0x25fbce5d1bcde2f9
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1-
          
          //1 : &Supprimer
          //2 : &Annuler
          SELON Dialogue(<§?15FC0070026E7D22§>)
          	// &Supprimer
          	CAS 1
          		
          		// Supprime le produit de la liste
          		gpclUneListe.ProduitSupprime(gsGUID)
          		ferme
          		
          	// &Annuler
          	CAS 2
          		retour
          FIN
         type : 32
   -
     name : OPT_FAVORI
     identifier : 0x25fbcd541b67d541
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          
          	// Favoris
          	SI FavoriExiste(gsGUID) ALORS
          		
          		// Supprime
          		si FavoriSupprime(gsGUID) alors
          		
          			<compile si TypeConfiguration = android>
          				OPT_FAVORI..Image = IMG_FAVORI_OFF
          				OPT_FAVORI..Libellé = <§Rff0005a9102b5123¤fr-FR¤Ajouter aux favoris§>
          			<sinon>
          				IMG_FAVORI = IMG_FAVORI_IOS_OFF
          				LIB_AJOUTER_FAVORIS..Libellé = <§Rff0005a9102f5161¤fr-FR¤Ajouter aux favoris§>		
          			<FIN>
          			
          			ToastAffiche(<§Rff0008af114a68c2¤fr-FR¤Produit supprimé des favoris§>, toastCourt, cvMilieu, chCentre)	
          		fin
          	SINON
          		
          		// Ajoute 
          		si FavoriAjoute(gsGUID) ALORS
          			
          			<COMPILE SI TypeConfiguration = Android>
          				OPT_FAVORI..Image = IMG_FAVORI_ON
          				OPT_FAVORI..Libellé = <§Rff0004a910295104¤fr-FR¤Enlever des favoris§>
          			<sinon>
          				IMG_FAVORI = IMG_FAVORI_IOS_ON
          				LIB_AJOUTER_FAVORIS..Libellé = <§Rff0004a9102d5142¤fr-FR¤Enlever des favoris§>		
          			<fin>
          			
          			ToastAffiche(<§Rff0009af114c68c2¤fr-FR¤Produit ajouté aux favoris§>, toastCourt, cvMilieu, chCentre)			
          		FIN
          	FIN
          
         type : 32
