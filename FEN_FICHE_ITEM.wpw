     code : |1-
      
      <compile SI MODE_IOS>
      	Actualise(faux)
      <FIN>
      
      // Synchronisation
      FichierVersEcran
      
      // Actualise la source du libellé des unités
      ActualiseLibelléUnité()
      
      // Image
      SI PAS gbCréation _et_ REQ_PRODUITS.Image = "" _OU_ fFichierExiste(DATA_IMAGE_PRODUCTS + [fSep] + REQ_PRODUITS.Image) = Faux ALORS
      	IMG_PICTO_PRODUIT = IMG_PRODUIT
      	IMG_PICTO_PRODUIT..note = ""
      SINON
      	IMG_PICTO_PRODUIT = DATA_IMAGE_PRODUCTS + [fSep] + REQ_PRODUITS.Image
      	IMG_PICTO_PRODUIT..Note = REQ_PRODUITS.Image
      FIN
      
      // Unité par défaut
      SI COMBO_UNITE <= 0 _ou_ ListeSelect(COMBO_UNITE) <= 0 ALORS
      	// QW267553 : contournement
      	<compile si TypeConfiguration =Android>
      		COMBO_UNITE = UNITEDEFAUT()
      	<sinon>
      		ListeSelectPlus(COMBO_UNITE, 1)
      	<FIN>
      FIN
      
      // En modification
      si PAS gbCréation alors
      		
      	// Favoris
      	SI FavoriExiste(gsGUID) ALORS
      		
      		<COMPILE SI TypeConfiguration = Android>
      			OPT_FAVORI..Image = IMG_FAVORI_ON
      			OPT_FAVORI..Libellé = <§Rff0004af113c68c2¤fr-FR¤Enlever des favoris§>
      		<SINON>
      			IMG_FAVORI = IMG_FAVORI_IOS_on
      			LIB_AJOUTER_FAVORIS..Libellé = <§Rff0004af113e68c2¤fr-FR¤Enlever des favoris§>		
      		<FIN>
      	SINON
      		
      		<COMPILE SI TypeConfiguration = Android>
      			OPT_FAVORI..Image = IMG_FAVORI_OFF
      			OPT_FAVORI..Libellé = <§Rff0005af114068c2¤fr-FR¤Ajouter aux favoris§>
      		<SINON>
      			IMG_FAVORI = IMG_FAVORI_ios_OFF
      			LIB_AJOUTER_FAVORIS..Libellé = <§Rff0005af114268c2¤fr-FR¤Ajouter aux favoris§>		
      		<FIN>
      	FIN		
      	
      	si gbestpersonnel = faux alors ecranpremier(SAI_QUANTITE)
      	
      // En création
      sinon
      	
      	SAI_LIBELLE 	= gpclProduit.Nom
      	SAI_NOTE		= gpclProduit.Description
      	SAI_CODEBARRE	= gpclProduit.Code
      	SAI_QUANTITE	= ""
      	
      	si gpclProduit.Image <> "" ALORS		
      		AttribueImage(imgURL, gpclProduit.Image)		
      	FIN		
      FIN
      
      // Fin de l'initialisation de la fenêtre
      gbinit = faux
