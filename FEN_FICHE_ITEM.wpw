      PROCEDURE AttribueImage(imgSource est un entier, Source)
      
      	bActualisé est un booléen
      	
      	// Calcule le chemin local pour la sauvegarde de l'image
      	sImageLocale est une chaîne = minuscule(gsGUID)
      	
      	selon imgSource
      		
      		// URL
      		cas imgURL
      			Sablier(Vrai)
      			
      			// Téléchargement
      			HTTPDestination(DATA_CACHE + [fSep] + sImageLocale + "_temp")
      			bActualisé = HTTPRequête(Source)
      			
      			Sablier(Faux)
      			
      		// Buffer
      		cas imgBuffer
      		
      			// Sauve la photo
      			bActualisé = fSauveBuffer(DATA_CACHE + [fSep] + sImageLocale + "_temp", Source)
      			
      		// Fichier
      		cas imgFichier
      					
      			// Copie la photo
      			bActualisé = fcopiefichier(Source, DATA_CACHE + [fSep] + sImageLocale + "_temp")
      	FIN
      	
      	
      	SI bActualisé ALORS
      		
      		// POST-Traitement
      		RedimensionnementAutomatique(DATA_CACHE + [fSep] + sImageLocale + "_temp")
      		
      		// Actualise
      		IMG_PICTO_PRODUIT..Note = sImageLocale
      		IMG_PICTO_PRODUIT = "" // Force la mise à jour
      		IMG_PICTO_PRODUIT = DATA_CACHE + [fSep] + sImageLocale + "_temp"
      		gbImageModifiée = vrai
      		
      	sinon
      		Erreur(<§Rff0007af114668c2¤fr-FR¤Impossible de récupérer l'image§>, ErreurInfo)
      	FIN		
      	
