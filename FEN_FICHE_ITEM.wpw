      // Fermeture du scanner
      SI MaFenêtreFille..Nom = "FEN_CB_SCAN" ALORS
      	
      	sCode est une chaine = mafenetrefille..ValeurRenvoyée
      	SI sCode <> "" ALORS
      		
      		// Détermine si le code barre a été actualisé ou non
      		bActualisé est un booléen = (SAI_CODEBARRE <> "" _et_ sCode <> SAI_CODEBARRE)
      		
      		SAI_CODEBARRE = sCode
      		
      		si bActualisé ALORS
      			
      			gbFicheModifiée = Vrai
      			
      			ToastAffiche(<§Rff0006af114468c2¤fr-FR¤Code-barres actualisé§>)
      		FIN
      	FIN
      	
      	retour
      FIN
      
      // Fermeture de la fenêtre des rayons
      SI MaFenêtreFille..Nom = "FEN_RAYONS" ALORS
      	
      	sGUID est une chaine = mafenetrefille..ValeurRenvoyée
      	SI sGUID <> COMBO_RAYON ALORS
      		
      		gbFicheModifiée = Vrai
      		
      		// Attribue le rayon
      		COMBO_RAYON = sGUID
      	FIN
      	
      	retour
      FIN
      
      // Fermeture de la fenêtre de sélection d'une liste (ajout rapide)
      si MaFenêtreFille..nom = "FEN_POP_LISTE" ALORS
      	
      	// Récupère la liste sélectionnée
      	nIDListe est un entier sur 8 = mafenetrefille..ValeurRenvoyée
      	si nIDListe > 0 ALORS
      		
      		// Alloue la liste
      		gpclUneListe = CListeShopping::Charge(nIDListe, "")
      		
      		// Vérifie si le produit est déjà présent dans la liste
      		HLitRecherchePremier(ListeContenu, ProduitListé, [nIDListe, gsGUID])
      		si htrouve(ListeContenu) ALORS
      			
      			// On modifiera la quantité
      			gbCréation = Vrai
      			
      		sinon
      			// On doit créer une entrée
      			HRAZ(ListeContenu)
      			gbCréation = vrai
      		FIN
      		
      		// Valide la fiche
      		FicheValide()
      	FIN
      	
      	retour
      FIN
