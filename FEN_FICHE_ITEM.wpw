     code : |1-
      // Résumé : Actualise les données
      // Syntaxe :
      //Actualise ( [<bUniquementVisibilité> est booléen])
      //
      // Paramètres :
      //	bUniquementVisibilité (booléen) : <indiquez ici le rôle de bUniquementVisibilité>
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE Actualise(bUniquementVisibilité est un booléen <utile> = enmodeios)
      
      	// Sous IOS, cache les options de menu avancé
      	// -> elle seront accessibles par le menu principal
      	<compile SI MODE_IOS>
      		
      		BTN_VALIDER..visible = faux
      		OPT_SUPPRIMER_PRODUIT..Visible = faux
      		OPT_FAVORI..Visible = faux
      		OPT_ENREGISTRER..visible = vrai
      		OPT_ENLEVER_LISTEACHAT..Visible = faux
      		
      		// Si on ne travaille qu'avec la visibilité
      		si bUniquementVisibilité ALORS			
      			
      			// Conserve les dimensions
      			HauteurZML = ZM_AVANCE..Hauteur
      			HauteurZM4 = ZM_AVANCE[4]..Hauteur			
      		FIN
      
      		IMG_FOND..Visible = (PAS gbCréation)		
      		
      		si pas bUniquementVisibilité ALORS
      			
      			// Cache les éléments de la ZML
      			ZM_AVANCE[1]..visible = (PAS gbCréation)						// Marge haute
      			ZM_AVANCE[5]..visible = (PAS gbCréation)						// Marge basse
      			ZM_AVANCE[2]..visible = (PAS gbCréation)						// Favoris
      			ZM_AVANCE[3]..visible = (PAS gbCréation)						// Supprimer de la liste
      			ZM_AVANCE[4]..visible = (PAS gbCréation) _ET_ gbEstPersonnel	// Supprimer le produit
      			ZM_AVANCE..Visible	  = (PAS gbCréation)						// ZML
      					
      			si gbCréation ALORS
      				
      				// Diminue la hauteur de la fenêtre
      				MaFenêtre..HauteurUtile -= HauteurZML
      				
      			sinon si pas gbEstPersonnel alors
      				
      				// Diminue la hauteur de la fenêtre
      				MaFenêtre..HauteurUtile -= HauteurZM4
      			FIN			
      		FIN
      
      	<sinon>
      		BTN_VALIDER..Visible = vrai
      		OPT_ENREGISTRER..visible = faux
      		ZM_AVANCE..Visible = Faux
      		OPT_SUPPRIMER_PRODUIT..Visible = (PAS gbCréation) _et_ gbEstPersonnel
      		OPT_FAVORI..Visible = (PAS gbCréation)
      		OPT_ENLEVER_LISTEACHAT..Visible = (pas gbCréation)
      		ZM_AVANCE..Visible = faux
      	<FIN>
      	
      	
