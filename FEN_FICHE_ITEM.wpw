      // Traitement automatique des exceptions :   exécuter le bloc de code CAS EXCEPTION:
      //
      
      PROCEDURE MaFenêtre(gpclUneListe est une CListeShopping dynamique, gsGUID est une chaine, gbCréation est un booléen = faux, gpclProduit est un EANProduit dynamique = null, gsGUIDParent est une chaine = "")
      
      gbEstPersonnel est un booléen = faux
      gbImageModifiée est un booléen = faux
      gbForceActualisation est un booléen = faux
      gbFicheModifiée est un booléen = faux
      gbInit est un booléen = vrai
      MonAlias est une chaine = moimeme..nom
      
      <COMPILE SI MODE_IOS>
      	HauteurZML	est un entier
      	HauteurZM4	est un entier	
      <FIN>
      
      // Observateur
      AjouteObservateur(ListeManager.SurRayonsModifiés, MonAlias, SurRayonsModifiés)
      
      mafenetre..ValeurRenvoyée = faux
      
      si pas gbCréation alors
      		
      	// On a une liste ?
      	si gpclUneListe <> null alors
      		
      		// Charge les infos du produit listé
      		HLitRecherchePremier(ListeContenu, ProduitListé, [gpclUneListe.IDListe, gsGUID])
      		si pas htrouve(ListeContenu) ALORS
      			ToastAffiche(<§Rff0001af113668c2¤fr-FR¤Produit inexistant§>)
      			ferme
      		FIN
      	fin
      	
      	// Charge le produit original
      	HLitRecherchePremier(REQ_PRODUITS, GUID, gsGUID)
      	SI PAS HTrouve(REQ_PRODUITS) ALORS
      		ToastAffiche(<§Rff0001a8102750d5¤fr-FR¤Produit inexistant§>)
      		Ferme
      	FIN
      	
      	// Vérifie si le produit est un produit créé par l'utilisateur
      	gbEstPersonnel = REQ_PRODUITS.EstPersonnel
      	
      	// Produit fixe
      	si pas gbEstPersonnel ALORS
      		
      		// Si on autorise la duplication des produits de la base
      		// -> vérifier que la requête REQ_SOURCE_LISTECONTENU permet d'éliminer les doublons sur GUID
      		si DUPLIQUE_PRODUIT_NATIF alors
      			// Pas de blocage
      			// -> c'est à la sauvegarde qu'on fera le nécessaire
      		sinon
      			
      			// Empêche la modification des données du produit
      			BTN_SCAN..etat = grise
      			BTN_MODIFIER_PHOTO..Visible = Faux
      			IMG_PICTO_PRODUIT..Etat = inactif
      			SAI_LIBELLE..etat = grise
      			SAI_CODEBARRE..etat = Grisé
      			COMBO_RAYON..etat = Grisé
      			IMG_CLIC_RAYON..Etat = Inactif
      		fin
      	sinon
      		
      		//
      		HLitRecherchePremier(Personnel, GUID, gsGUID)
      	FIN
      sinon
      	mafenetre..titre = <§Rff0002af113868c2¤fr-FR¤Nouveau§>
      	
      	gbEstPersonnel = vrai
      	gbFicheModifiée = vrai
      	
      	hraz(ListeContenu)
      	hraz(personnel)
      	hraz(REQ_PRODUITS)
      	
      	// Crée une entrée
      	Personnel.GUID 		= gsGUID
      	Personnel.GUIDParent= gsGUIDParent
      	Personnel.Produit 	= Vrai
      	Personnel.Nom		= gpclProduit.Nom
      	Personnel.NomIndex	= ChaîneVersIndex(Personnel.Nom)
      	Personnel.Langue	= Nation
      	Personnel.Code		= gpclProduit.Code
      fin
      
      Actualise()
      ecranpremier(BTN_SCAN)
      
      CAS EXCEPTION:
      	xlog("[FEN_FICHE_ITEM] " + exceptioninfo(errcomplet))
      	
      	MaFenêtre..ValeurRenvoyée = Faux
      	ToastAffiche(<§Rff0003af113a68c2¤fr-FR¤Une erreur inattendue est survenue§>, toastcourt, cvmilieu, chcentre)
      	ferme
