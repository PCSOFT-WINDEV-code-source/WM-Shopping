     name : FicheValide
     internal_properties : CAAAAAgAAAB0z7CG6XAwgdrqV3WqRivS5+SEbHoDZb7QKjTFpdw6K4ZT8dTBsgDtEHjqlb64eU6ikl5j3ihNAzrmdeZUJkx5qVK49NiQLxbAyMIHr2T7aPnWNuho/cLhrwTfZ8IXjy7exdamT5A85ytOfJv6Nrfaw6GlDLbj5v3mE/Isq3fVSDSOyDw1VHvmgu8x96ryK4OFPtmUjwQ=
     procedure_id : 1614551289733277422
     type_code : 14
     code : |1-
      // Résumé : Valide la fiche
      // Syntaxe :
      // FicheValide ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE FicheValide()
      
      	bDuplication est un booléen
      	bForceMAJ est un booléen
      	
      	// Création/Edition de produit
      	SI gpclUneListe = Null ALORS
      
      		// Demande la sélection d'une liste
      		OuvreFenêtreMobile(FEN_POP_LISTE)
      		RETOUR		
      	FIN
      
      
      	// Rayon
      	sGUIDRayon est une chaîne = COMBO_RAYON..ValeurMémorisée
      	SI sGUIDRayon = -1 		ALORS sGUIDRayon = ""
      	SI sGUIDRayon = "ZZZ" 	ALORS sGUIDRayon = ""
      	
      	// Vérifie la quantité saisie
      	si SAI_QUANTITE = "" _OU_ Val(SAI_QUANTITE) = 0 ALORS
      		
      		// Pas de quantité saisie alors qu'un unité l'a été
      		si pas COMBO_UNITE..ValeurMémorisée dans (0, "", UNITEDEFAUT()) ALORS
      			
      			// On refuse
      			ToastAffiche(<§Rff000eaf114868c2¤fr-FR¤Veuillez saisir une quantité !§>, toastCourt, cvHaut, chCentre)
      			DonneFocus(SAI_QUANTITE)
      			retour
      		FIN
      	FIN
      	
      	////////////////////////
      	// C'est un produit livré avec l'application
      	////////////////////////
      	SI PAS gbCréation _et_ gbEstPersonnel = faux ALORS
      		
      		// Duplication autorisée
      		SI DUPLIQUE_PRODUIT_NATIF ALORS
      			
      			// On vérifie les changements effectués
      			si (SAI_LIBELLE <> REQ_PRODUITS.Nom) _ou_ (sGUIDRayon <> REQ_PRODUITS.GUIDParent) _ou_ (SAI_CODEBARRE <> REQ_PRODUITS.CodeBarre) _ou_ gbImageModifiée ALORS
      				
      				////////////////////////
      				// On a édité une propriété du produit, donc on doit dupliquer le produit
      				////////////////////////
      
      				// On passe en mode création
      				gbEstPersonnel = Vrai
      				bDuplication = vrai
      				
      				HRAZ(Personnel)
      				
      				// Réécrit les valeurs des GUIDs
      				gsGUID = REQ_PRODUITS.GUID
      				gsGUIDParent = REQ_PRODUITS.GUIDParent
      				gpclProduit = allouer un EANProduit
      				gpclProduit.Code = REQ_PRODUITS.CodeBarre
      				
      				// Crée une entrée en recopiant les données (même GUID)
      				Personnel.GUID 		= gsGUID
      				Personnel.GUIDParent= gsGUIDParent
      				Personnel.Produit 	= Vrai
      				Personnel.Nom		= REQ_PRODUITS.Nom
      				Personnel.NomIndex	= REQ_PRODUITS.NomIndex
      				Personnel.Langue	= Nation
      				Personnel.Code		= REQ_PRODUITS.CodeBarre
      				Personnel.Image 	= REQ_PRODUITS.Image			
      			FIN		
      		fin
      	FIN
      	
      	// Mise à jour de produit dans la liste
      	SI PAS gbCréation _ET_ gpclUneListe <> Null ALORS
      		
      		// Mise à jour pour la liste
      		EcranVersFichier(FEN_FICHE_ITEM, ListeContenu)
      		ListeContenu.GUIDCatégorie = RayonParent(sGUIDRayon)
      		
      		// RAZ
      		// Quantité nulle ou 1 pièce (unité par défaut)
      		SI SAI_QUANTITE = "" _OU_ (Val(SAI_QUANTITE) <= 1 _ET_ ListeContenu.IDUnité DANS (0, UNITEDEFAUT())) ALORS
      			
      			// N'affiche pas la quantité
      			ListeContenu.IDUnité = 0
      			ListeContenu.Quantité = 1
      		FIN
      		
      		// Quantité "virtuelle" pour le comptage
      		ListeContenu.QuantitéVirtuelle = (ListeContenu.IDUnité DANS (0, UNITEDEFAUT()) ? ListeContenu.Quantité sinon 1)
      		
      		ListeContenu.IDModification = USERID
      		ListeContenu.DateModification = DateHeureSys
      		
      		// Enregistre
      		HEnregistre(ListeContenu)
      	FIN
      	
      	// Produit non fixe
      	SI gbEstPersonnel ALORS
      		
      		// Renomme l'image temporaire	
      		SI gbImageModifiée ALORS		
      			SI IMG_PICTO_PRODUIT..Note <> "" ALORS
      				fCopieFichier(DATA_CACHE + [fSep] + IMG_PICTO_PRODUIT..Note + "_temp", DATA_IMAGE_PRODUCTS + [fSep] + IMG_PICTO_PRODUIT..Note)
      			FIN
      		FIN
      		
      		// Détermine si on a modifié les données du produit
      		bDonnéesModifiées est un booléen
      		bDonnéesModifiées = gbCréation _ou_ bDuplication _ou_ gbImageModifiée _ou_ (Personnel.Nom <> SAI_LIBELLE _OU_ Personnel.Code <> SAI_CODEBARRE _OU_  Personnel.GUIDParent <> sGUIDRayon)
      		
      		// Si les données du produit ont été modifiées
      		SI bDonnéesModifiées ALORS
      			
      			// Mise à jour
      			Personnel.Nom 		 = SAI_LIBELLE
      			Personnel.NomIndex	 = ChaîneVersIndex(Personnel.Nom)
      			Personnel.Code 		 = SAI_CODEBARRE
      			Personnel.GUIDParent = sGUIDRayon
      			
      			SI gbImageModifiée ALORS
      				Personnel.Image = IMG_PICTO_PRODUIT..Note
      			FIN
      			
      			// Enregistre
      			HEnregistre(Personnel)
      			
      			// Mise à jour
      			bForceMAJ = vrai			
      		FIN
      	FIN
      	
      	// Mise à jour de la note
      	SI PAS SAI_NOTE ~= "" ALORS
      		
      		// Création de la note associée
      		HLitRecherchePremier(Note, GUID, gsGUID)
      		SI PAS HTrouve(Note) ALORS
      			HRAZ(Note)
      		FIN
      		
      		Note.GUID = gsGUID
      		Note.Note = SAI_NOTE
      		
      		// Enregistre
      		HEnregistre(Note)
      	FIN
      	
      	// Si on doit forcer une mise à jour des données sources
      	si bForceMAJ  ALORS
      		
      		// Mise à jour des sources de données
      		SourceActualise(Faux)
      	FIN
      	
      	// Création de produit
      	SI gbCréation _ET_ gpclUneListe <> Null ALORS
      		
      		// Ajoute
      		ListeAjouteProduit(gsGUID, gpclUneListe)
      	FIN
      	
      	// Valide les modifications
      	MaFenêtre..ValeurRenvoyée = Vrai
      	Ferme(FEN_FICHE_ITEM)
     type : 458752
