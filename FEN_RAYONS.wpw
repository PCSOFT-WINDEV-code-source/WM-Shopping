   -
     name : FinSurbrillance
     procedure_id : 1638639867616605455
     type_code : 14
     code : |1-
      PROCEDURE FinSurbrillance(LOCAL nLigneSurbrillance est un entier)
      
      	// Déclenche la fin de surbrillance
      	CurseurHolo = 1
      	FinSurbrillanceItem(nLigneSurbrillance)
     type : 458752
   -
     name : FinSurbrillanceItem
     internal_properties : CAAAAAgAAABiNQvbf418G6aleWOHU6gMtMoztyXCPnZvseK7aKMV9An6N6r+tOhLOffl/3pTfS5Heza6xPcgIxTm68p/djvqXmuzsmIFPbi193k0MoLXa/qmM3L714xcChIRV3scTJiM9dRkKOg78f3lnEAFZ/C1vRSULAiAjfAlAf8ZSYNceXTy
     procedure_id : 1638639867616670991
     type_code : 14
     code : |1-
      // Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code, avec un différé de 1 seconde
      // Elle sera répétée en boucle, en attendant 5 centièmes de seconde entre chaque appel
      // Un nouveau timer est créé pour chaque appel à la procédure
      // 
      PROCEDURE FinSurbrillanceItem(LOCAL nLigneSurbrillance est un entier)
      
      	CurseurHolo ++
      	
      	si CurseurHolo <= Surbrillance..Occurrence ALORS
      		// Mise à jour de la ligne
      		SurbrillanceLigne(nLigneSurbrillance, Surbrillance[CurseurHolo])
      	fin
      
      	// Fin du timer
      	si CurseurHolo >= Surbrillance..Occurrence ALORS
      		FinAutomatismeProcédure()
      	FIN
     type : 458752
   -
     name : SurbrillanceLigne
     procedure_id : 1638639867616736527
     type_code : 14
     code : |1-
      PROCEDURE SurbrillanceLigne(LOCAL nLigneZR est un entier, LOCAL nCouleur est un entier)
      
      	// Mise à jour de la couleur de fond
      	si 1 <= nLigneZR <= ZR_RAYONS..Occurrence ALORS
      		ZR_RAYONS[nLigneZR].IMG_FONDLIGNE..CouleurFond = nCouleur		
      	FIN
     type : 458752
   -
     name : AfficheRayons
     procedure_id : 1638639867616802063
     type_code : 14
     code : |1-
      // Résumé : Affiche les rayons
      // Syntaxe :
      //[ <Résultat> = ] AfficheRayons ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	entier : Indice à positionner
      //
      PROCEDURE AfficheRayons()
      
      	stUnRayon est un STRayon
      	nUnRayon est un entier
      	nRayon est un entier
      	
      	// RAZ
      	supprimetout(SourceRecherche)
      	zonerepeteesupprimetout(ZR_RAYONS)
      	
      	// Remplissage
      	POUR TOUT stUnRayon DE MesRayons
      		
      		// Ajoute une ligne
      		nUnRayon = ZoneRépétéeAjouteLigne(ZR_RAYONS, stUnRayon.GUID, stUnRayon.Nom, stUnRayon.Perso)
      								
      		// Met en évidence le rayon actuellement sélectionné
      		SI stUnRayon.GUID = GUIRayonSélectionné ALORS
      			ZR_RAYONS[nUnRayon].LIB_RAYON..PoliceGras = Vrai
      			nRayon = nUnRayon
      		FIN
      		
      		// Conserve le nom du rayon pour la recherche
      		Ajoute(SourceRecherche, ChaîneFormate(stUnRayon.Nom, ccSansPonctuationNiEspace + ccMinuscule + ccSansAccent))
      	FIN
      	
      renvoyer nRayon
     type : 458752
   -
     name : SurRayonsModifiés
     procedure_id : 1638639867616867599
     type_code : 14
     code : |1-
      // Résumé : Mise à jour des rayons
      // Syntaxe :
      //SurRayonsModifiés ( [<VOID>])
      //
      // Paramètres :
      // 	VOID (valeur par défaut=0) : NULL
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SurRayonsModifiés(VOID <utile> = NULL)
      	
      	// Affiche les rayons
      	nRayon est un entier = AfficheRayons()
      	
      	// Positionne l'ascenseur à l'emplacement du rayon
      	ZoneRépétéePosition(ZR_RAYONS, Max(1, nRayon - 1))	
     type : 458752
