     type : 165
   -
     internal_properties : CAAAAAgAAABkYxCOAWHWSEcmS47+Q4zr76DFeUhRJGaNs29bIcM+i9P48K1hoWenUlLTUcG+n+jT4Jtjp2sXjnWCcDud5gVpkt1ArZhMBRqjw7AWM4YjS6jH+jMDU88Vva+NPKIVwCdEi8l9zIuOl9Q91Q==
     code : |1-
      
      // Edition du rayon
      SI MaFenêtreFille..Nom = "FEN_RAYON_EDITE" ALORS
      	
      	sGUID est une chaine = mafenetrefille..ValeurRenvoyée
      	SI sGUID <> "" ALORS
      		
      		// Recopie les rayons
      		TableauCopie(MesRayons, rayons)
      		
      		// Actualise les rayons
      		AfficheRayons()
      		
      		// Recherche le rayon édité
      		nIndice est un entier = zonerepeteeCherche(ATT_GUID, sGUID)
      		SI nIndice > 0 ALORS
      			
      			// Positionne sur le rayon
      			ZoneRépétéePosition(ZR_RAYONS, Max(1, nIndice - 1))
      			
      			// Met en évidence la ligne
      			SurbrillanceLigne(nIndice, Surbrillance[1])	
      			FinSurbrillance(nIndice)
      		FIN
      	FIN
      FIN
