        p_codes :
         -
           code : |1-
            
            // Il ne s'agit pas d'un rayon "perso"
            si pas EstPersonnel ALORS
            	
            	// Annule le balayage en cours
            	ZoneRépétéeAnnuleBalayage(ZoneRépétée)
            sinon
            
            	// Supprime le rayon
            	sGUIDParent est une chaine = ListeManager.RayonSupprime(GUIDRayon)
            			
            	// Si le rayon supprimé est le rayon sélectionné
            	si FEN_RAYONS.GUIRayonSélectionné = GUIDRayon ALORS
            		
            		// On modifie le rayon sélectionné
            		FEN_RAYONS.GUIRayonSélectionné = sGUIDParent
            		
            		// Met en évidence le rayon
            		nUnRayon est un entier = ZoneRépétéeCherche(FEN_RAYONS.ZR_RAYONS.ATT_GUID, sGUIDParent)
            		FEN_RAYONS.ZR_RAYONS[nUnRayon].LIB_RAYON..PoliceGras = Vrai
            	FIN
            	
            	// La suppression de la ligne entraîne la fermeture de la fenêtre interne
            	// elle doit être la dernière ligne de code à exécuter
            	ZoneRépétéeSupprime(ZoneRépétée, gnIndice)
            FIN
