     p_codes :
      -
        code : |1-
         PROCEDURE MaFenêtre(ZoneRépétée, gnIndice est un entier)
         
         // Cette fenêtre interne est chargée depuis <FEN_RAYONS.ZR_RAYONS>
         EstPersonnel est un booléen = FEN_RAYONS.ZR_RAYONS.ATT_PERSONNEL[gnIndice]
         GUIDRayon    est une chaine = FEN_RAYONS.ZR_RAYONS.ATT_GUID[gnindice]
         
         // Il ne s'agit pas d'un rayon "perso"
         si pas EstPersonnel _ou_ pas FEN_RAYONS.AjoutPossible ALORS
         	
         	// Désactive les options
         	BTN_SUPPR..Visible = faux
         	BTN_EDIT..Visible = Faux	
         	
         	// Force la largeur minimale
         	moimeme..Largeur = 0
         FIN
      -
        code : |1-
         
         // Il ne s'agit pas d'un rayon "perso"
         SI pas EstPersonnel ALORS
         	
         	// Annule le balayage en cours
         	ZoneRépétéeAnnuleBalayage(ZoneRépétée)
         FIN
