  procedures :
   -
     name : AttribueImage
     internal_properties : 
     procedure_id : 1585636633625146438
     type_code : 14
     code : |1-
      PROCEDURE AttribueImage(imgSource est un entier, Source)
      
      	bActualisé est un booléen
      	sNomListe est une chaine = ChaîneFormate(SAI_NOM_LISTE, ccSansAccent+ccSansPonctuationNiEspace+ccMinuscule)
      	
      	// Aucun nom spécifié
      	si sNomListe = "" ALORS
      		
      		// Utilise un GUID
      		sNomListe = DonneGUID(guidBrut)
      	sinon
      		
      		// Préfixe le nom de la liste
      		sNomListe = "user_" + sNomListe
      	FIN
      	
      	// Calcule le chemin local pour la sauvegarde de l'image
      	sImageLocale est une chaîne = sNomListe
      	
      	selon imgSource
      		
      		// URL
      		cas imgURL
      			Sablier(Vrai)
      			
      			// Téléchargement
      			HTTPDestination(DATA_CACHE + [fSep] + sImageLocale + "_temp")
      			bActualisé = HTTPRequête(Source)
      			
      			Sablier(Faux)
      			
      		// Buffer
      		cas imgBuffer
      		
      			// Sauve la photo
      			bActualisé = fSauveBuffer(DATA_CACHE + [fSep] + sImageLocale + "_temp", Source)
      			
      		// Fichier
      		cas imgFichier
      		
      			// Copie la photo
      			bActualisé = fCopieFichier(Source, DATA_CACHE + [fSep] + sImageLocale + "_temp")
      	FIN
      	
      	
      	SI bActualisé ALORS
      		
      		// POST-Traitement
      		RedimensionnementAutomatique(DATA_CACHE + [fSep] + sImageLocale + "_temp")
      				
      		// Actualise
      		IMG_PICTO_LISTE..Note = sImageLocale
      		IMG_PICTO_LISTE = "" // Force la mise à jour
      		IMG_PICTO_LISTE = DATA_CACHE + [fSep] + sImageLocale + "_temp"
      		gbImageModifiée = vrai
      		
      	sinon
      		Erreur(<§Rff0000af10f868c2¤fr-FR¤Impossible de récupérer l'image§>, ErreurInfo)
      	FIN		
      	
     type : 458752
   -
     name : SurSélectionImage
     internal_properties : CAAAAAgAAABSov83baYAB3BXg9MD9y3thcgjGAFM9/y0xPiUV1ce9y6eRvbNpnwYJnR1CoK/o4koYScq3TnxjKtOeoR0VgocpxvY/RT1if1EoxUx9z9XL5ib1PyIw4yrS2DbK4Bp/VhysUqKvzIa1cVIk8y9X7TVEGb8lWPko7itItFRjoY661b4Kprr6BVwHnNF87aCRD+eR5HIH4cI6G6FAt0S7WVmHazcdKw1
     procedure_id : 1585636633625212193
     type_code : 14
     code : |1+
      PROCEDURE SurSélectionImage(sNouvellePhoto est une chaine)
      
      	SI sNouvellePhoto <> "" ALORS
      		
      		// Attribue l'image
      		AttribueImage(imgFichier, sNouvellePhoto)
      		
      		<COMPILE SI MODE_IOS>
      			// Les images, photos et vidéos récupérées par AlbumSélecteur sont copiées dans le répertoire "temp" de l'application.
      			// On doit donc les supprimer
      			fSupprime(sNouvellePhoto)
      		<FIN>
      	FIN
     type : 458752
