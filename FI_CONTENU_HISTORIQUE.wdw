  procedures :
   -
     name : ActualiseContenuHistorique
     procedure_id : 1576040645933745816
     type_code : 14
     code : |1-
      PROCEDURE ActualiseContenuHistorique(VOID <utile> = null)
      
      	// Vide la liste
      	ZoneRépétéeSupprimeTout(ZR_HISTORIQUE)
      	
      	// Récupère le contenu de l'historique
      	SI HExécuteRequête(REQ_HISTORIQUE_CONTENU) ALORS
      		
      		Items est un tableau de ListeElément
      		stElément est un ListeElément
      				
      		HLitPremier(REQ_HISTORIQUE_CONTENU)
      		tantque pas hendehors(REQ_HISTORIQUE_CONTENU)
      		
      			stElément.GUIDRayon = REQ_HISTORIQUE_CONTENU.GUIDCatégorie
      			stElément.Rayon = RayonLibellé(stElément.GUIDRayon)
      			stElément.GUIDProduit = REQ_HISTORIQUE_CONTENU.GUIDProduit
      			stElément.Image = REQ_HISTORIQUE_CONTENU.Image
      			stElément.Produit = remplace(REQ_HISTORIQUE_CONTENU.NomProduit, "&", "&&")
      			stElément.Quantité = REQ_HISTORIQUE_CONTENU.Compteur
      						
      			ajoute(Items, stElément)	
      			hlitsuivant(REQ_HISTORIQUE_CONTENU)		
      		fin
      				
      		// Tri général
      		trie(Items, ttFonction, TriListeContenu)
      					
      		pour tout stElément de Items
      			
      			nLigne est un entier = ZoneRépétéeAjouteLigne(ZR_HISTORIQUE, stElément.GUIDRayon, 0, stElément.GUIDProduit, stElément.Quantité)
      			
      			ZR_HISTORIQUE[nLigne].LIB_RAYON						= stElément.Rayon
      			ZR_HISTORIQUE[nLigne].LIB_PRODUIT 					= stElément.Produit
      			ZR_HISTORIQUE[nLigne].IMG_EX_VALIDER..visible		= gpclUneListe.ProduitEstAcheté((stElément.GUIDProduit))
      			ZR_HISTORIQUE[nLigne].IMG_FAVORI..visible			= FavoriExiste(stElément.GUIDProduit)
      			
      			si stElément.Image = "" _ou_ fFichierExiste(DATA_IMAGE_PRODUCTS + [fSep] + stElément.Image) = faux ALORS
      				ZR_HISTORIQUE[nLigne].IMG_PICTO_PRODUIT 			= IMG_PRODUIT
      			sinon
      				ZR_HISTORIQUE[nLigne].IMG_PICTO_PRODUIT 			= DATA_IMAGE_PRODUCTS + [fSep] + stElément.Image
      			FIN
      		FIN
      	sinon
      		XLOG("[ActualiseContenuHistorique] (REQ_HISTORIQUE_CONTENU) Erreur = " + herreurinfo)
      	FIN
      	
      	// Libellé vide
      	GR_AIDE_HISTORIQUEVIDE..Visible = (ZR_HISTORIQUE..Occurrence = 0)	
     type : 458752
   -
     name : TriListeContenu
     procedure_id : 1576040645933811352
     type_code : 14
     code : |1-
      PROCEDURE TriListeContenu(stElt1 est un ListeElément, stElt2 est un ListeElément)
      
      	// Tri :
      	// - En priorité, tri par rayon
      	// - Puis par date d'ajout
      	
      	sRayon1 est une chaine = stElt1.GUIDRayon
      	sRayon2 est une chaine = stElt2.GUIDRayon
      	
      	// Si le produit 1 est acheté
      	si stElt1.Acheté ALORS		
      		// On le bascule dans un autre rayon
      		sRayon1 = "ZZZ"
      	FIN
      	
      	// Si le produit 2 est acheté
      	si stElt2.Acheté ALORS
      		// On le bascule dans un autre rayon
      		sRayon2 = "ZZZ"
      	FIN
      		
      	// Deux rayons différents
      	si sRayon1 <> sRayon2 alors
      		
      		nPos1 est un entier = 0
      		nPos2 est un entier = 0
      		
      		SI stElt1.Acheté ALORS nPos1 = 999999999
      		SI stElt2.Acheté ALORS nPos2 = 999999999
      		
      		// Produit hors-rayon
      		si sRayon1 = "" alors nPos1 = 999999998
      		si sRayon2 = "" alors nPos2 = 999999998
      					
      		// Selon la position
      		si nPos1 > nPos2 alors renvoyer 1
      		si nPos1 < nPos2 alors renvoyer -1		
      	fin	
      		
      	// Produit 1 favori, Produit 2 pas favori
      	SI FavoriExiste(stElt1.GUIDProduit) _et_ pas FavoriExiste(stElt2.GUIDProduit) ALORS renvoyer -1
      	
      	// Produit 2 favori, Produit 1 pas favori
      	SI FavoriExiste(stElt2.GUIDProduit) _et_ pas FavoriExiste(stElt1.GUIDProduit) ALORS renvoyer 1
      	
      	// Si on arrive ici, tri par date
      	si stElt1.DateAjout > stElt2.DateAjout alors renvoyer 1
      	si stElt1.DateAjout < stElt2.DateAjout alors renvoyer -1
      	
      	// Si on arrive ici, tri par ordre alphabétique
      	renvoyer ChaîneCompare(stElt1.Produit, stElt2.Produit, ccSansCasse)
     type : 458752
